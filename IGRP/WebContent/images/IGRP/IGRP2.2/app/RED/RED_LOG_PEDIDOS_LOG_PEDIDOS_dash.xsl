        <!--START PAGE--><form action="#" method="post" id="formular_default" name="formular_default" class="default_form"  enctype="multipart/form-data"><!--FORMULARIO --><!--START FORM--><!--TOOL BAR--><xsl:apply-templates mode="tools-bar" select="rows/content/form/tools-bar" /><div class="box-content"><!--HIDDEN FIELDS--><xsl:apply-templates mode="form-hidden" select="rows/content/form/value" /><xsl:if test="rows/content/form/label/referencia"><label><span><xsl:value-of select="rows/content/form/label/referencia"/></span><input type="text" name="{rows/content/form/value/referencia/@name}" value="{rows/content/form/value/referencia}"   maxlength="30"  class=""><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/referencia" /></xsl:call-template></input></label></xsl:if><xsl:if test="rows/content/form/label/nome"><label><span><xsl:value-of select="rows/content/form/label/nome"/></span><input type="text" name="{rows/content/form/value/nome/@name}" value="{rows/content/form/value/nome}"   maxlength="150"  class=""><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/nome" /></xsl:call-template></input></label></xsl:if><xsl:if test="rows/content/form/label/descricao"><label><span><xsl:value-of select="rows/content/form/label/descricao"/></span><textarea name="{rows/content/form/value/descricao/@name}"  maxlength="1000"  class=""><xsl:value-of select="rows/content/form/value/descricao"/><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/descricao" /></xsl:call-template></textarea></label></xsl:if><xsl:if test="rows/content/form/label/data_identificacao"><label><span><xsl:value-of select="rows/content/form/label/data_identificacao"/></span><input type="text" name="{rows/content/form/value/data_identificacao/@name}" value="{rows/content/form/value/data_identificacao}"  class="IGRP_datepicker date"><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/data_identificacao" /></xsl:call-template></input></label></xsl:if><xsl:if test="rows/content/form/label/remetente"><label><span><xsl:value-of select="rows/content/form/label/remetente"/></span><input type="text" name="{rows/content/form/value/remetente/@name}" value="{rows/content/form/value/remetente}"   maxlength="150"  class=""><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/remetente" /></xsl:call-template></input></label></xsl:if><xsl:if test="rows/content/form/label/dependencia"><label><a id="idp_dependencia" /><span><xsl:value-of select="rows/content/form/label/dependencia"/></span><xsl:for-each select="rows/content/form/list/dependencia/option"><br /><input type="radio" name="{../@name}" value="{value}" class="IGRP_change"><xsl:if test="@selected='true'"><xsl:attribute name="checked">checked</xsl:attribute></xsl:if></input><xsl:value-of select="text"/></xsl:for-each></label></xsl:if><xsl:if test="rows/content/form/label/referencia_dep"><label><span><xsl:value-of select="rows/content/form/label/referencia_dep"/></span><input type="text" name="{rows/content/form/value/referencia_dep/@name}" value="{rows/content/form/value/referencia_dep}"   maxlength="30"  class=""><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/referencia_dep" /></xsl:call-template></input><xsl:call-template name="lookup-tool"><xsl:with-param name="page" select="rows/page" /><xsl:with-param name="ad_hoc" select="'1'" /><xsl:with-param name="action" select="'LOOKUP'" /><xsl:with-param name="name" select="rows/content/form/value/referencia_dep/@name" /><xsl:with-param name="js_lookup" select="rows/content/form/lookup/referencia_dep" /></xsl:call-template></label></xsl:if><xsl:if test="rows/content/form/label/prioridade_inicial"><label><span><xsl:value-of select="rows/content/form/label/prioridade_inicial"/></span><input type="text" name="{rows/content/form/value/prioridade_inicial/@name}" value="{rows/content/form/value/prioridade_inicial}"   maxlength="30"  class=""><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/prioridade_inicial" /></xsl:call-template></input></label></xsl:if><xsl:if test="rows/content/form/label/prioridade_revista"><label><span><xsl:value-of select="rows/content/form/label/prioridade_revista"/></span><input type="text" name="{rows/content/form/value/prioridade_revista/@name}" value="{rows/content/form/value/prioridade_revista}"   maxlength="30"  class=""><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/prioridade_revista" /></xsl:call-template></input></label></xsl:if><xsl:if test="rows/content/form/label/data_fim_esperado"><label><span><xsl:value-of select="rows/content/form/label/data_fim_esperado"/></span><input type="text" name="{rows/content/form/value/data_fim_esperado/@name}" value="{rows/content/form/value/data_fim_esperado}"  class="IGRP_datepicker date"><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/data_fim_esperado" /></xsl:call-template></input></label></xsl:if><xsl:if test="rows/content/form/label/areas_afectados"><xsl:call-template name="page-title"><xsl:with-param name="title" select="rows/content/form/label/areas_afectados" /><xsl:with-param name="class" select="'subtitle'" /></xsl:call-template></xsl:if><xsl:if test="rows/content/form/label/areas_afectados"><xsl:call-template name="tools-bar-form-group"><xsl:with-param name="rel" select="'areas_afectados'" /></xsl:call-template></xsl:if><xsl:if test="rows/content/form/label/areas"><label><span><xsl:value-of select="rows/content/form/label/areas"/></span><select name="{rows/content/form/list/areas/@name}" rel="F_areas_afectados"  class=""><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/areas" /></xsl:call-template><xsl:for-each select="rows/content/form/list/areas/option"><option value="{value}"><xsl:if test="@selected='true'"><xsl:attribute name="selected">selected</xsl:attribute></xsl:if><xsl:value-of select="text"/></option></xsl:for-each></select></label></xsl:if><xsl:if test="rows/content/form/label/areas_afectados"><!--INICIO DA TABELA--><div class="simple-table"><table rel="T_areas_afectados"><!--CABECALHO DA TABELA--><thead><tr><xsl:if test="not(rows/content/form/table/areas_afectados/label/areas/@visible)"><th><xsl:value-of select="rows/content/form/table/areas_afectados/label/areas"/></th></xsl:if><th align="center" width="10"></th><th align="center" width="10"></th></tr></thead><!--CORPO DA TABELA--><tbody><xsl:for-each select="rows/content/form/table/areas_afectados/value/row"><tr><xsl:if test="not(areas/@visible)"><td><xsl:value-of select="areas_desc" /><input type="hidden" name="p_areas_fk" value="{areas}" /><input type="hidden" name="p_areas_fk_desc" value="{areas_desc}" /></td></xsl:if><xsl:if test="areas/@visible='false'"><input type="hidden" name="p_areas_fk" value="{areas}" /><input type="hidden" name="p_areas_fk_desc" value="{areas_desc}" /></xsl:if><td align="center" width="10"><input type="hidden" name="p_areas_afectados_id" value="{areas_afectados_id}" /><xsl:if test="not(@nodelete)"><img alt="apagar" src="{$path}/img/icon/ctx-delete.png" class="IGRP_delRow" rel="areas_afectados"/></xsl:if></td><td align="center" width="10"><xsl:if test="not(@noupdate)"><img alt="editar" src="{$path}/img/icon/edit.png" class="IGRP_editRow" rel="areas_afectados"/></xsl:if></td></tr></xsl:for-each></tbody></table></div><!--FIM DA TABELA--></xsl:if></div><!--END FORM--></form><!--END PAGE-->


