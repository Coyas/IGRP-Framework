		  <!--START PAGE--><!--SIMPLE-MENU--><xsl:apply-templates mode="menu-simple" select="rows/content/menu" /><form action="#" method="post" id="formular_default" name="formular_default" class="default_filter"><!--FILTRO --><!--START FORM--><div class="box-content"><!--HIDDEN FIELDS--><xsl:apply-templates mode="form-hidden" select="rows/content/filter/value" /><label><span><xsl:value-of select="rows/content/filter/label/rnits_id"/></span><select name="{rows/content/filter/list/rnits_id/@name}" class=""><xsl:for-each select="rows/content/filter/list/rnits_id/option"><option value="{value}"><xsl:if test="@selected='true'"><xsl:attribute name="selected">selected</xsl:attribute></xsl:if><xsl:value-of select="text"/></option></xsl:for-each></select></label><label><span><xsl:value-of select="rows/content/filter/label/acto"/></span><input type="text" name="{rows/content/filter/value/acto/@name}" value="{rows/content/filter/value/acto}"  class="" /></label><label><span><xsl:value-of select="rows/content/filter/label/tipo"/></span><input type="checkbox" name="{rows/content/filter/value/tipo/@name}" value="1"  class=""><xsl:if test="rows/content/filter/value/tipo='1'"><xsl:attribute name="checked">checked</xsl:attribute></xsl:if></input></label><label><span><xsl:value-of select="rows/content/filter/label/codigo"/></span><input type="number" name="{rows/content/filter/value/codigo/@name}" value="{rows/content/filter/value/codigo}"  class="number" /></label><label><span><xsl:value-of select="rows/content/filter/label/acto_id"/></span><input type="number" name="{rows/content/filter/value/acto_id/@name}" value="{rows/content/filter/value/acto_id}"  class="number" /></label><label><span><xsl:value-of select="rows/content/filter/label/prazo_dias"/></span><input type="number" name="{rows/content/filter/value/prazo_dias/@name}" value="{rows/content/filter/value/prazo_dias}"  class="number" /></label><label><span><xsl:value-of select="rows/content/filter/label/prazo_dias_pn"/></span><input type="number" name="{rows/content/filter/value/prazo_dias_pn/@name}" value="{rows/content/filter/value/prazo_dias_pn}"  class="number" /></label><label><span><xsl:value-of select="rows/content/filter/label/prazo_dias_pd"/></span><input type="number" name="{rows/content/filter/value/prazo_dias_pd/@name}" value="{rows/content/filter/value/prazo_dias_pd}"  class="number" /></label><label><span><xsl:value-of select="rows/content/filter/label/cancelar"/></span><input type="checkbox" name="{rows/content/filter/value/cancelar/@name}" value="1"  class=""><xsl:if test="rows/content/filter/value/cancelar='1'"><xsl:attribute name="checked">checked</xsl:attribute></xsl:if></input></label><label><span><xsl:value-of select="rows/content/filter/label/fg_recibo"/></span><input type="checkbox" name="{rows/content/filter/value/fg_recibo/@name}" value="1"  class=""><xsl:if test="rows/content/filter/value/fg_recibo='1'"><xsl:attribute name="checked">checked</xsl:attribute></xsl:if></input></label><label><span><xsl:value-of select="rows/content/filter/label/ex_geografia"/></span><input type="text" name="{rows/content/filter/value/ex_geografia/@name}" value="{rows/content/filter/value/ex_geografia}"  class="" /></label><label><span><xsl:value-of select="rows/content/filter/label/rnio_id"/></span><input type="number" name="{rows/content/filter/value/rnio_id/@name}" value="{rows/content/filter/value/rnio_id}"  class="number" /></label><!--TOOLBAR FILTER--><xsl:apply-templates mode="button-bar" select="rows/content/filter/tools-bar" /></div><!--END FORM--><!--TOOL BAR--><xsl:apply-templates mode="tools-bar" select="rows/content/table/tools-bar" /><!-- CONTEXT-MENU --><xsl:apply-templates mode="context-menu" select="rows/content/table/context-menu"/><!--INICIO DA TABELA--><div class="table"><table class="sortable"><!--CABECALHO DA TABELA--><thead><tr><xsl:if test="rows/content/table/label/id"><th align="center"><xsl:value-of select="rows/content/table/label/id"/></th></xsl:if><xsl:if test="rows/content/table/label/rnits_id"><th align="center"><xsl:value-of select="rows/content/table/label/rnits_id"/></th></xsl:if><xsl:if test="rows/content/table/label/acto"><th align="left"><xsl:value-of select="rows/content/table/label/acto"/></th></xsl:if><xsl:if test="rows/content/table/label/tipo"><th class="checkbox sorttable_nosort" align="center"><input type="checkbox" name="p_tipo_all" value="{value}" class="IGRP_checkall" /></th></xsl:if><xsl:if test="rows/content/table/label/codigo"><th align="center"><xsl:value-of select="rows/content/table/label/codigo"/></th></xsl:if><xsl:if test="rows/content/table/label/acto_id"><th align="center"><xsl:value-of select="rows/content/table/label/acto_id"/></th></xsl:if><xsl:if test="rows/content/table/label/prazo_dias"><th align="center"><xsl:value-of select="rows/content/table/label/prazo_dias"/></th></xsl:if><xsl:if test="rows/content/table/label/prazo_dias_pn"><th align="center"><xsl:value-of select="rows/content/table/label/prazo_dias_pn"/></th></xsl:if><xsl:if test="rows/content/table/label/prazo_dias_pd"><th align="center"><xsl:value-of select="rows/content/table/label/prazo_dias_pd"/></th></xsl:if><xsl:if test="rows/content/table/label/cancelar"><th class="checkbox sorttable_nosort" align="center"><input type="checkbox" name="p_cancelar_all" value="{value}" class="IGRP_checkall" /></th></xsl:if><xsl:if test="rows/content/table/label/fg_recibo"><th class="checkbox sorttable_nosort" align="center"><input type="checkbox" name="p_fg_recibo_all" value="{value}" class="IGRP_checkall" /></th></xsl:if><xsl:if test="rows/content/table/label/ex_geografia"><th align="left"><xsl:value-of select="rows/content/table/label/ex_geografia"/></th></xsl:if><xsl:if test="rows/content/table/label/rnio_id"><th align="center"><xsl:value-of select="rows/content/table/label/rnio_id"/></th></xsl:if></tr></thead><!--CORPO DA TABELA--><tbody><xsl:for-each select="rows/content/table/value/row"><xsl:variable name="pos" select="(position()+1) mod 2"/><tr class="IGRP_contextMenu"><xsl:apply-templates mode="context-param" select="context-menu" /><xsl:if test="id"><td class="table_row{$pos}" align="center"><xsl:value-of select="id" /></td></xsl:if><xsl:if test="rnits_id"><td class="table_row{$pos}" align="center"><u class="IGRP_lookup" title="Passa Valor"><xsl:value-of select="rnits_id" /></u></td></xsl:if><xsl:if test="acto"><td class="table_row{$pos} checkbox" align="left"><a href="{acto}" class="_blank">acto</a></td></xsl:if><xsl:if test="tipo"><td class="table_row{$pos} checkbox" align="left"><input type="checkbox" name="p_tipo" value="{tipo}"><xsl:if test="tipo_check=tipo"><xsl:attribute name="checked">checked</xsl:attribute></xsl:if></input><xsl:value-of select="tipo_desc" /></td></xsl:if><xsl:if test="codigo"><td class="table_row{$pos}" align="center"><xsl:value-of select="codigo" /></td></xsl:if><xsl:if test="acto_id"><td class="table_row{$pos}" align="center"><xsl:value-of select="acto_id" /></td></xsl:if><xsl:if test="prazo_dias"><td class="table_row{$pos}" align="center"><xsl:value-of select="prazo_dias" /></td></xsl:if><xsl:if test="prazo_dias_pn"><td class="table_row{$pos}" align="center"><xsl:value-of select="prazo_dias_pn" /></td></xsl:if><xsl:if test="prazo_dias_pd"><td class="table_row{$pos}" align="center"><xsl:value-of select="prazo_dias_pd" /></td></xsl:if><xsl:if test="cancelar"><td class="table_row{$pos} checkbox" align="left"><input type="checkbox" name="p_cancelar" value="{cancelar}"><xsl:if test="cancelar_check=cancelar"><xsl:attribute name="checked">checked</xsl:attribute></xsl:if></input><xsl:value-of select="cancelar_desc" /></td></xsl:if><xsl:if test="fg_recibo"><td class="table_row{$pos} checkbox" align="left"><input type="checkbox" name="p_fg_recibo" value="{fg_recibo}"><xsl:if test="fg_recibo_check=fg_recibo"><xsl:attribute name="checked">checked</xsl:attribute></xsl:if></input><xsl:value-of select="fg_recibo_desc" /></td></xsl:if><xsl:if test="ex_geografia"><td class="table_row{$pos}" align="left"><xsl:value-of select="ex_geografia" /></td></xsl:if><xsl:if test="rnio_id"><td class="table_row{$pos}" align="center"><xsl:value-of select="rnio_id" /></td></xsl:if></tr></xsl:for-each></tbody></table></div><!--FIM DA TABELA--></form><!--END PAGE-->


