        <!--NOTIFICATION MESSAGES-->
        <xsl:apply-templates mode="notif_messages" select="rows/content/messages" >
          <xsl:with-param name="class" select="'notification'"/>
        </xsl:apply-templates>


        <!--START FORM--><!--TOOL BAR--><xsl:apply-templates mode="tools-bar" select="rows/content/form/tools-bar" /><div class="box-content"><form action="#" method="post" id="formular_default" name="formular_default" class="default_form" enctype="multipart/form-data"><!--HIDDEN FIELDS--><xsl:apply-templates mode="form-hidden" select="rows/content/form/value" /><!--Devo abrir uma div aqui --><div id="startPage"><xsl:if test="rows/content/form/label/teste_texto"><label><span><xsl:value-of select="rows/content/form/label/teste_texto"/></span><input type="text" name="{rows/content/form/value/teste_texto/@name}" value="{rows/content/form/value/teste_texto}"   maxlength="30"  class=""><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/teste_texto" /></xsl:call-template></input></label></xsl:if></div><!--FIM DO SEPARADOR--><!--INICIO DO SEPARADOR "SEPADOR_DIALOG"--><div id="{local-name(rows/content/form/label/sepador_dialog)}"><xsl:if test="rows/content/form/label/sepador_dialog"><xsl:call-template name="page-title"><xsl:with-param name="title" select="rows/content/form/label/sepador_dialog" /><xsl:with-param name="class" select="'subtitle'" /></xsl:call-template></xsl:if><xsl:if test="rows/content/form/label/sepador_dialog"><xsl:call-template name="tools-bar-form-group"><xsl:with-param name="rel" select="'sepador_dialog'" /><xsl:with-param name="class" select="'IGRP_addRow_diag'" /></xsl:call-template></xsl:if><!--SEPARATORDIALOG sepador_dialog--><div class="IGRP_overlay"></div><div class="D_sepador_dialog IGRP_dialogbox"><xsl:if test="rows/content/form/label/sepador_dialog"><xsl:call-template name="page-title"><xsl:with-param name="title" select="rows/content/form/label/sepador_dialog" /></xsl:call-template></xsl:if><xsl:if test="rows/content/form/label/texto"><label><span><xsl:value-of select="rows/content/form/label/texto"/></span><input type="text" name="{rows/content/form/value/texto/@name}" value="{rows/content/form/value/texto}"  rel="F_sepador_dialog"   maxlength="30"  class=""><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/texto" /></xsl:call-template></input></label></xsl:if><xsl:if test="rows/content/form/label/numero"><label><a id="idp_numero" /><span><xsl:value-of select="rows/content/form/label/numero"/></span><input type="number" name="{rows/content/form/value/numero/@name}" value="{rows/content/form/value/numero}"  rel="F_sepador_dialog"   maxlength="30"  class="number IGRP_change"><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/numero" /></xsl:call-template></input></label></xsl:if><xsl:if test="rows/content/form/label/data"><label><span><xsl:value-of select="rows/content/form/label/data"/></span><input type="text" name="{rows/content/form/value/data/@name}" value="{rows/content/form/value/data}"  rel="F_sepador_dialog"  class="IGRP_datepicker date"><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/data" /></xsl:call-template></input></label></xsl:if><div class="dialog_btnContainer"><input type="button" class="IGRP_row_confirm" value="Confirmar" rel="sepador_dialog"/><input type="button" class="IGRP_btn_close" value="Fechar" rel="sepador_dialog"/></div></div><!--devo fachar DIV_DIALOG aqui--><xsl:if test="rows/content/form/label/sepador_dialog"><!--INICIO DA TABELA  rows/content/form/table/sepador_dialog/--><div class="simple-table"><table rel="T_sepador_dialog"><!--CABECALHO DA TABELA p.COUNT= 3--><thead><tr><xsl:if test="not(rows/content/form/table/sepador_dialog/label/texto/@visible)"><th><xsl:value-of select="rows/content/form/table/sepador_dialog/label/texto"/></th></xsl:if><xsl:if test="not(rows/content/form/table/sepador_dialog/label/numero/@visible)"><th><xsl:value-of select="rows/content/form/table/sepador_dialog/label/numero"/></th></xsl:if><xsl:if test="not(rows/content/form/table/sepador_dialog/label/data/@visible)"><th><xsl:value-of select="rows/content/form/table/sepador_dialog/label/data"/></th></xsl:if><th align="center" width="10"></th><th align="center" width="10"></th></tr></thead><!--CORPO DA TABELA--><tbody><xsl:for-each select="rows/content/form/table/sepador_dialog/value/row"><tr><xsl:if test="not(texto/@visible)"><td><xsl:value-of select="texto_desc" /><input type="hidden" name="p_texto_fk" value="{texto}" /><input type="hidden" name="p_texto_fk_desc" value="{texto_desc}" /></td></xsl:if><xsl:if test="texto/@visible='false'"><input type="hidden" name="p_texto_fk" value="{texto}" /><input type="hidden" name="p_texto_fk_desc" value="{texto_desc}" /></xsl:if><xsl:if test="not(numero/@visible)"><td><xsl:value-of select="numero_desc" /><input type="hidden" name="p_numero_fk" value="{numero}" /><input type="hidden" name="p_numero_fk_desc" value="{numero_desc}" /></td></xsl:if><xsl:if test="numero/@visible='false'"><input type="hidden" name="p_numero_fk" value="{numero}" /><input type="hidden" name="p_numero_fk_desc" value="{numero_desc}" /></xsl:if><xsl:if test="not(data/@visible)"><td><xsl:value-of select="data_desc" /><input type="hidden" name="p_data_fk" value="{data}" /><input type="hidden" name="p_data_fk_desc" value="{data_desc}" /></td></xsl:if><xsl:if test="data/@visible='false'"><input type="hidden" name="p_data_fk" value="{data}" /><input type="hidden" name="p_data_fk_desc" value="{data_desc}" /></xsl:if><td align="center" width="10"><input type="hidden" name="p_sepador_dialog_id" value="{sepador_dialog_id}" /><xsl:if test="not(@nodelete)"><img alt="apagar" src="{$path}/img/icon/ctx-delete.png" class="IGRP_delRow" rel="sepador_dialog"/></xsl:if></td><td align="center" width="10"><xsl:if test="not(@noupdate)"><img alt="editar" src="{$path}/img/icon/edit.png" class="IGRP_editRow" rel="sepador_dialog"/></xsl:if></td></tr></xsl:for-each></tbody></table></div><!--FIM DA TABELA--></xsl:if></div><!--FIM DO SEPARADOR--><!--INICIO DO SEPARADOR "SEPARADOR_TESTE"--><div id="{local-name(rows/content/form/label/separador_teste)}"><xsl:if test="rows/content/form/label/separador_teste"><xsl:call-template name="page-title"><xsl:with-param name="title" select="rows/content/form/label/separador_teste" /><xsl:with-param name="class" select="'subtitle'" /></xsl:call-template></xsl:if><xsl:if test="rows/content/form/label/nome"><label><span><xsl:value-of select="rows/content/form/label/nome"/></span><input type="text" name="{rows/content/form/value/nome/@name}" value="{rows/content/form/value/nome}"   maxlength="30"  class=""><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/nome" /></xsl:call-template></input></label></xsl:if></div><!--FIM DO SEPARADOR--><!--INICIO DO SEPARADOR "SEPARADOR_DIALOG_2"--><div id="{local-name(rows/content/form/label/separador_dialog_2)}"><xsl:if test="rows/content/form/label/separador_dialog_2"><xsl:call-template name="page-title"><xsl:with-param name="title" select="rows/content/form/label/separador_dialog_2" /><xsl:with-param name="class" select="'subtitle'" /></xsl:call-template></xsl:if><xsl:if test="rows/content/form/label/separador_dialog_2"><xsl:call-template name="tools-bar-form-group"><xsl:with-param name="rel" select="'separador_dialog_2'" /><xsl:with-param name="class" select="'IGRP_addRow_diag'" /></xsl:call-template></xsl:if><!--SEPARATORDIALOG separador_dialog_2--><div class="IGRP_overlay"></div><div class="D_separador_dialog_2 IGRP_dialogbox"><xsl:if test="rows/content/form/label/separador_dialog_2"><xsl:call-template name="page-title"><xsl:with-param name="title" select="rows/content/form/label/separador_dialog_2" /></xsl:call-template></xsl:if><xsl:if test="rows/content/form/label/bi"><label><a id="idp_bi" /><span><xsl:value-of select="rows/content/form/label/bi"/></span><input type="text" name="{rows/content/form/value/bi/@name}" value="{rows/content/form/value/bi}"  rel="F_separador_dialog_2"   maxlength="30"  class="IGRP_change"><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/bi" /></xsl:call-template></input><xsl:call-template name="lookup-tool"><xsl:with-param name="page" select="'LOOKUP'" /><xsl:with-param name="action" select="'LOOKUP_BI'" /><xsl:with-param name="name" select="rows/content/form/value/bi/@name" /><xsl:with-param name="js_lookup" select="rows/content/form/lookup/bi" /></xsl:call-template></label></xsl:if><div class="dialog_btnContainer"><input type="button" class="IGRP_row_confirm" value="Confirmar" rel="separador_dialog_2"/><input type="button" class="IGRP_btn_close" value="Fechar" rel="separador_dialog_2"/></div></div><!--devo fachar DIV_DIALOG aqui--><xsl:if test="rows/content/form/label/separador_dialog_2"><!--INICIO DA TABELA  rows/content/form/table/separador_dialog_2/--><div class="simple-table"><table rel="T_separador_dialog_2"><!--CABECALHO DA TABELA p.COUNT= 2--><thead><tr><xsl:if test="not(rows/content/form/table/separador_dialog_2/label/bi/@visible)"><th><xsl:value-of select="rows/content/form/table/separador_dialog_2/label/bi"/></th></xsl:if><th align="center" width="10"></th><th align="center" width="10"></th></tr></thead><!--CORPO DA TABELA--><tbody><xsl:for-each select="rows/content/form/table/separador_dialog_2/value/row"><tr><xsl:if test="not(bi/@visible)"><td><xsl:value-of select="bi_desc" /><input type="hidden" name="p_bi_fk" value="{bi}" /><input type="hidden" name="p_bi_fk_desc" value="{bi_desc}" /></td></xsl:if><xsl:if test="bi/@visible='false'"><input type="hidden" name="p_bi_fk" value="{bi}" /><input type="hidden" name="p_bi_fk_desc" value="{bi_desc}" /></xsl:if><td align="center" width="10"><input type="hidden" name="p_separador_dialog_2_id" value="{separador_dialog_2_id}" /><input type="hidden" name="p_bi_id_fk" value="{bi_id}" /><input type="hidden" name="p_bi_id_fk_desc" value="{bi_id_desc}" /><xsl:if test="not(@nodelete)"><img alt="apagar" src="{$path}/img/icon/ctx-delete.png" class="IGRP_delRow" rel="separador_dialog_2"/></xsl:if></td><td align="center" width="10"><xsl:if test="not(@noupdate)"><img alt="editar" src="{$path}/img/icon/edit.png" class="IGRP_editRow" rel="separador_dialog_2"/></xsl:if></td></tr></xsl:for-each></tbody></table></div><!--FIM DA TABELA--></xsl:if></div><!--devo fachar DIV_SEPARATOR aqui--></form></div><!--END FORM-->


     </div>


