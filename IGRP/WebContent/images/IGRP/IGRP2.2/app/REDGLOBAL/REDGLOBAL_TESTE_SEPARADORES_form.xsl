        <!--START FORM--><!--TOOL BAR--><xsl:apply-templates mode="tools-bar" select="rows/content/form/tools-bar" /><div class="box-content"><form action="#" method="post" id="formular_default" name="formular_default" class="default_form" enctype="multipart/form-data"><!--HIDDEN FIELDS--><xsl:apply-templates mode="form-hidden" select="rows/content/form/value" /><!--INICIO DO SEPARADOR "SEPARADOR_LISTA"--><div id="{local-name(rows/content/form/label/separador_lista)}"><xsl:if test="rows/content/form/label/separador_lista"><xsl:call-template name="page-title"><xsl:with-param name="title" select="rows/content/form/label/separador_lista" /><xsl:with-param name="class" select="'subtitle'" /></xsl:call-template></xsl:if><xsl:if test="rows/content/form/label/separador_lista"><xsl:call-template name="tools-bar-form-group"><xsl:with-param name="rel" select="'separador_lista'" /></xsl:call-template></xsl:if><xsl:if test="rows/content/form/label/numero"><label><span>(*) <xsl:value-of select="rows/content/form/label/numero"/></span><select name="{rows/content/form/list/numero/@name}" rel="F_separador_lista"  multiple="multiple" size="5" class="required"><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/numero" /></xsl:call-template><xsl:for-each select="rows/content/form/list/numero/option"><option value="{value}"><xsl:if test="@selected='true'"><xsl:attribute name="selected">selected</xsl:attribute></xsl:if><xsl:value-of select="text"/></option></xsl:for-each></select></label></xsl:if><xsl:if test="rows/content/form/label/texto"><label><span>(*) <xsl:value-of select="rows/content/form/label/texto"/></span><input type="text" name="{rows/content/form/value/texto/@name}" value="{rows/content/form/value/texto}"  rel="F_separador_lista"   maxlength="30"  class="required"><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/texto" /></xsl:call-template></input></label></xsl:if><xsl:if test="rows/content/form/label/check_lista"><label><span>(*) <xsl:value-of select="rows/content/form/label/check_lista"/></span><xsl:for-each select="rows/content/form/list/check_lista/option"><br /><input type="checkbox" name="{../@name}" value="{value}" rel="F_separador_lista"  class="required"><xsl:if test="@selected='true'"><xsl:attribute name="checked">checked</xsl:attribute></xsl:if><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="../../../label/check_lista" /></xsl:call-template></input><xsl:value-of select="text"/></xsl:for-each></label></xsl:if><xsl:if test="rows/content/form/label/data"><label><span>(*) <xsl:value-of select="rows/content/form/label/data"/></span><input type="text" name="{rows/content/form/value/data/@name}" value="{rows/content/form/value/data}"  rel="F_separador_lista"  class="IGRP_datepicker required date"><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/data" /></xsl:call-template></input></label></xsl:if><xsl:if test="rows/content/form/label/separador_lista"><!--INICIO DA TABELA  rows/content/form/table/separador_lista/--><div class="simple-table"><table rel="T_separador_lista"><!--CABECALHO DA TABELA --><thead><tr><xsl:if test="rows/content/form/table/separador_lista/label/numero"><xsl:if test="not(rows/content/form/table/separador_lista/label/numero/@visible)"><th  ><xsl:value-of select="rows/content/form/table/separador_lista/label/numero"/></th></xsl:if></xsl:if><xsl:if test="rows/content/form/table/separador_lista/label/texto"><xsl:if test="not(rows/content/form/table/separador_lista/label/texto/@visible)"><th  ><xsl:value-of select="rows/content/form/table/separador_lista/label/texto"/></th></xsl:if></xsl:if><xsl:if test="rows/content/form/table/separador_lista/label/check_lista"><xsl:if test="not(rows/content/form/table/separador_lista/label/check_lista/@visible)"><th  ><xsl:value-of select="rows/content/form/table/separador_lista/label/check_lista"/></th></xsl:if></xsl:if><xsl:if test="rows/content/form/table/separador_lista/label/data"><xsl:if test="not(rows/content/form/table/separador_lista/label/data/@visible)"><th  ><xsl:value-of select="rows/content/form/table/separador_lista/label/data"/></th></xsl:if></xsl:if><th align="center" width="10"></th><th align="center" width="10"></th></tr></thead><!--CORPO DA TABELA--><tbody><xsl:for-each select="rows/content/form/table/separador_lista/value/row[not(@total='yes')]"><tr><xsl:if test="numero"><xsl:if test="not(numero/@visible)"><td><xsl:value-of select="numero_desc" /><input type="hidden" name="p_numero_fk" value="{numero}" /><input type="hidden" name="p_numero_fk_desc" value="{numero_desc}" /></td></xsl:if></xsl:if><xsl:if test="numero/@visible='false'"><input type="hidden" name="p_numero_fk" value="{numero}" /><input type="hidden" name="p_numero_fk_desc" value="{numero_desc}" /></xsl:if><xsl:if test="texto"><xsl:if test="not(texto/@visible)"><td><xsl:value-of select="texto_desc" /><input type="hidden" name="p_texto_fk" value="{texto}" /><input type="hidden" name="p_texto_fk_desc" value="{texto_desc}" /></td></xsl:if></xsl:if><xsl:if test="texto/@visible='false'"><input type="hidden" name="p_texto_fk" value="{texto}" /><input type="hidden" name="p_texto_fk_desc" value="{texto_desc}" /></xsl:if><xsl:if test="check_lista"><xsl:if test="not(check_lista/@visible)"><td><xsl:value-of select="check_lista_desc" /><input type="hidden" name="p_check_lista_fk" value="{check_lista}" /><input type="hidden" name="p_check_lista_fk_desc" value="{check_lista_desc}" /></td></xsl:if></xsl:if><xsl:if test="check_lista/@visible='false'"><input type="hidden" name="p_check_lista_fk" value="{check_lista}" /><input type="hidden" name="p_check_lista_fk_desc" value="{check_lista_desc}" /></xsl:if><xsl:if test="data"><xsl:if test="not(data/@visible)"><td><xsl:value-of select="data_desc" /><input type="hidden" name="p_data_fk" value="{data}" /><input type="hidden" name="p_data_fk_desc" value="{data_desc}" /></td></xsl:if></xsl:if><xsl:if test="data/@visible='false'"><input type="hidden" name="p_data_fk" value="{data}" /><input type="hidden" name="p_data_fk_desc" value="{data_desc}" /></xsl:if><td align="center" width="10"><input type="hidden" name="p_separador_lista_id" value="{separador_lista_id}" /><xsl:if test="not(@nodelete)"><img alt="apagar" src="{$path}/img/icon/ctx-delete.png" class="IGRP_delRow" rel="separador_lista"/></xsl:if></td><td align="center" width="10"><xsl:if test="not(@noupdate)"><img alt="editar" src="{$path}/img/icon/edit.png" class="IGRP_editRow" rel="separador_lista"/></xsl:if></td></tr></xsl:for-each></tbody></table><!--LEGENDA FINAL DA TABELA --><xsl:apply-templates mode="legend_color" select="rows/content/form/table/separador_lista/legend_color"/></div><!--FIM DA TABELA--></xsl:if></div><!--FIM DO SEPARADOR--><!--INICIO DO SEPARADOR "SEPARADOR_DIALOG"--><div id="{local-name(rows/content/form/label/separador_dialog)}"><xsl:if test="rows/content/form/label/separador_dialog"><xsl:call-template name="page-title"><xsl:with-param name="title" select="rows/content/form/label/separador_dialog" /><xsl:with-param name="class" select="'subtitle'" /></xsl:call-template></xsl:if><xsl:if test="rows/content/form/label/separador_dialog"><xsl:call-template name="tools-bar-form-group"><xsl:with-param name="rel" select="'separador_dialog'" /><xsl:with-param name="class" select="'IGRP_addRow_diag'" /></xsl:call-template></xsl:if><!--SEPARATORDIALOG separador_dialog--><div class="IGRP_overlay"></div><div class="D_separador_dialog IGRP_dialogbox"><xsl:if test="rows/content/form/label/separador_dialog"><div class="IGRP_dialogboxTop"><div class="close"><img src="{$path_tmpl}/img/close-dialog.png" class="IGRP_btn_close" rel="separador_dialog"/></div><xsl:call-template name="page-title"><xsl:with-param name="title" select="rows/content/form/label/separador_dialog" /></xsl:call-template></div></xsl:if><div class="IGRP_dialogboxCenter"><xsl:if test="rows/content/form/label/numero_dlg"><label><span><xsl:value-of select="rows/content/form/label/numero_dlg"/></span><input type="text" name="{rows/content/form/value/numero_dlg/@name}" value="{rows/content/form/value/numero_dlg}"  rel="F_separador_dialog"   maxlength="30"  class=""><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/numero_dlg" /></xsl:call-template></input></label></xsl:if><xsl:if test="rows/content/form/label/texto_dlg"><label><span>(*) <xsl:value-of select="rows/content/form/label/texto_dlg"/></span><input type="text" name="{rows/content/form/value/texto_dlg/@name}" value="{rows/content/form/value/texto_dlg}"  rel="F_separador_dialog"   maxlength="30"  class="required"><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/texto_dlg" /></xsl:call-template></input></label></xsl:if><xsl:if test="rows/content/form/label/data_dlg"><label><span>(*) <xsl:value-of select="rows/content/form/label/data_dlg"/></span><input type="text" name="{rows/content/form/value/data_dlg/@name}" value="{rows/content/form/value/data_dlg}"  rel="F_separador_dialog"   maxlength="30"  class="required"><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/data_dlg" /></xsl:call-template></input></label></xsl:if><xsl:if test="rows/content/form/label/check_lista_dlg"><label><span>(*) <xsl:value-of select="rows/content/form/label/check_lista_dlg"/></span><xsl:for-each select="rows/content/form/list/check_lista_dlg/option"><br /><input type="checkbox" name="{../@name}" value="{value}" rel="F_separador_dialog"  class="required"><xsl:if test="@selected='true'"><xsl:attribute name="checked">checked</xsl:attribute></xsl:if><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="../../../label/check_lista_dlg" /></xsl:call-template></input><xsl:value-of select="text"/></xsl:for-each></label></xsl:if></div><div class="dialog_btnContainer"><input type="button" class="IGRP_row_confirm" value="Confirmar" rel="separador_dialog"/><input type="button" class="IGRP_row_confirmClose" value="Confirmar e Fechar" rel="separador_dialog"/></div></div><!--devo fachar DIV_DIALOG aqui--><xsl:if test="rows/content/form/label/separador_dialog"><!--INICIO DA TABELA  rows/content/form/table/separador_dialog/--><div class="simple-table"><table rel="T_separador_dialog"><!--CABECALHO DA TABELA --><thead><tr><xsl:if test="rows/content/form/table/separador_dialog/label/numero_dlg"><xsl:if test="not(rows/content/form/table/separador_dialog/label/numero_dlg/@visible)"><th  ><xsl:value-of select="rows/content/form/table/separador_dialog/label/numero_dlg"/></th></xsl:if></xsl:if><xsl:if test="rows/content/form/table/separador_dialog/label/texto_dlg"><xsl:if test="not(rows/content/form/table/separador_dialog/label/texto_dlg/@visible)"><th  ><xsl:value-of select="rows/content/form/table/separador_dialog/label/texto_dlg"/></th></xsl:if></xsl:if><xsl:if test="rows/content/form/table/separador_dialog/label/data_dlg"><xsl:if test="not(rows/content/form/table/separador_dialog/label/data_dlg/@visible)"><th  ><xsl:value-of select="rows/content/form/table/separador_dialog/label/data_dlg"/></th></xsl:if></xsl:if><xsl:if test="rows/content/form/table/separador_dialog/label/check_lista_dlg"><xsl:if test="not(rows/content/form/table/separador_dialog/label/check_lista_dlg/@visible)"><th  ><xsl:value-of select="rows/content/form/table/separador_dialog/label/check_lista_dlg"/></th></xsl:if></xsl:if><th align="center" width="10"></th><th align="center" width="10"></th></tr></thead><!--CORPO DA TABELA--><tbody><xsl:for-each select="rows/content/form/table/separador_dialog/value/row[not(@total='yes')]"><tr><xsl:if test="numero_dlg"><xsl:if test="not(numero_dlg/@visible)"><td><xsl:value-of select="numero_dlg_desc" /><input type="hidden" name="p_numero_dlg_fk" value="{numero_dlg}" /><input type="hidden" name="p_numero_dlg_fk_desc" value="{numero_dlg_desc}" /></td></xsl:if></xsl:if><xsl:if test="numero_dlg/@visible='false'"><input type="hidden" name="p_numero_dlg_fk" value="{numero_dlg}" /><input type="hidden" name="p_numero_dlg_fk_desc" value="{numero_dlg_desc}" /></xsl:if><xsl:if test="texto_dlg"><xsl:if test="not(texto_dlg/@visible)"><td><xsl:value-of select="texto_dlg_desc" /><input type="hidden" name="p_texto_dlg_fk" value="{texto_dlg}" /><input type="hidden" name="p_texto_dlg_fk_desc" value="{texto_dlg_desc}" /></td></xsl:if></xsl:if><xsl:if test="texto_dlg/@visible='false'"><input type="hidden" name="p_texto_dlg_fk" value="{texto_dlg}" /><input type="hidden" name="p_texto_dlg_fk_desc" value="{texto_dlg_desc}" /></xsl:if><xsl:if test="data_dlg"><xsl:if test="not(data_dlg/@visible)"><td><xsl:value-of select="data_dlg_desc" /><input type="hidden" name="p_data_dlg_fk" value="{data_dlg}" /><input type="hidden" name="p_data_dlg_fk_desc" value="{data_dlg_desc}" /></td></xsl:if></xsl:if><xsl:if test="data_dlg/@visible='false'"><input type="hidden" name="p_data_dlg_fk" value="{data_dlg}" /><input type="hidden" name="p_data_dlg_fk_desc" value="{data_dlg_desc}" /></xsl:if><xsl:if test="check_lista_dlg"><xsl:if test="not(check_lista_dlg/@visible)"><td><xsl:value-of select="check_lista_dlg_desc" /><input type="hidden" name="p_check_lista_dlg_fk" value="{check_lista_dlg}" /><input type="hidden" name="p_check_lista_dlg_fk_desc" value="{check_lista_dlg_desc}" /></td></xsl:if></xsl:if><xsl:if test="check_lista_dlg/@visible='false'"><input type="hidden" name="p_check_lista_dlg_fk" value="{check_lista_dlg}" /><input type="hidden" name="p_check_lista_dlg_fk_desc" value="{check_lista_dlg_desc}" /></xsl:if><td align="center" width="10"><input type="hidden" name="p_separador_dialog_id" value="{separador_dialog_id}" /><xsl:if test="not(@nodelete)"><img alt="apagar" src="{$path}/img/icon/ctx-delete.png" class="IGRP_delRow" rel="separador_dialog"/></xsl:if></td><td align="center" width="10"><xsl:if test="not(@noupdate)"><img alt="editar" src="{$path}/img/icon/edit.png" class="IGRP_editRow" rel="separador_dialog"/></xsl:if></td></tr></xsl:for-each></tbody></table><!--LEGENDA FINAL DA TABELA --><xsl:apply-templates mode="legend_color" select="rows/content/form/table/separador_dialog/legend_color"/></div><!--FIM DA TABELA--></xsl:if></div><!-- "FORM" devo fachar DIV_SEPARATOR aqui--></form></div><!--END FORM-->


