ws/content/form/label/bi"><label><span>(*) <xsl:value-of select="rows/content/form/label/bi"/></span><input type="number" name="{rows/content/form/value/bi/@name}" value="{rows/content/form/value/bi}"  rel="F_sep_lista"   maxlength="22"  class="required number"><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/bi" /></xsl:call-template></input></label></xsl:if><xsl:if test="rows/content/form/label/data_nasc"><label><span><xsl:value-of select="rows/content/form/label/data_nasc"/></span><input type="text" name="{rows/content/form/value/data_nasc/@name}" value="{rows/content/form/value/data_nasc}"  rel="F_sep_lista"  class="IGRP_datepicker date"><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/data_nasc" /></xsl:call-template></input></label></xsl:if><xsl:if test="rows/content/form/label/sexo"><label><span>(*) <xsl:value-of select="rows/content/form/label/sexo"/></span><select name="{rows/content/form/list/sexo/@name}" rel="F_sep_lista"  class="required"><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/sexo" /></xsl:call-template><xsl:for-each select="rows/content/form/list/sexo/option"><option value="{value}"><xsl:if test="@selected='true'"><xsl:attribute name="selected">selected</xsl:attribute></xsl:if><xsl:value-of select="text"/></option></xsl:for-each></select></label></xsl:if><xsl:if test="rows/content/form/label/nome_pai"><label><span><xsl:value-of select="rows/content/form/label/nome_pai"/></span><input type="text" name="{rows/content/form/value/nome_pai/@name}" value="{rows/content/form/value/nome_pai}"  rel="F_sep_lista"   maxlength="100"  class=""><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/nome_pai" /></xsl:call-template></input></label></xsl:if><xsl:if test="rows/content/form/label/nome_mae"><label><span><xsl:value-of select="rows/content/form/label/nome_mae"/></span><input type="text" name="{rows/content/form/value/nome_mae/@name}" value="{rows/content/form/value/nome_mae}"  rel="F_sep_lista"   maxlength="100"  class=""><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/nome_mae" /></xsl:call-template></input></label></xsl:if><xsl:if test="rows/content/form/label/email"><label><span><xsl:value-of select="rows/content/form/label/email"/></span><input type="email" name="{rows/content/form/value/email/@name}" value="{rows/content/form/value/email}"  rel="F_sep_lista"   maxlength="100"  class="email"><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/email" /></xsl:call-template></input></label></xsl:if><xsl:if test="rows/content/form/label/morada"><label><span><xsl:value-of select="rows/content/form/label/morada"/></span><input type="text" name="{rows/content/form/value/morada/@name}" value="{rows/content/form/value/morada}"  rel="F_sep_lista"   maxlength="200"  class=""><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/morada" /></xsl:call-template></input><xsl:call-template name="lookup-tool"><xsl:with-param name="page" select="'LOOKUP'" /><xsl:with-param name="action" select="'LOOKUP_GEOG'" /><xsl:with-param name="name" select="rows/content/form/value/morada/@name" /><xsl:with-param name="js_lookup" select="rows/content/form/lookup/morada" /></xsl:call-template></label></xsl:if><xsl:if test="rows/content/form/label/sep_lista"><!--INICIO DA TABELA--><div class="simple-table"><table rel="T_sep_lista"><!--CABECALHO DA TABELA--><thead><tr><xsl:if test="not(rows/content/form/table/sep_lista/label/nome/@visible)"><th><xsl:value-of select="rows/content/form/table/sep_lista/label/nome"/></th></xsl:if><xsl:if test="not(rows/content/form/table/sep_lista/label/nif/@visible)"><th><xsl:value-of select="rows/content/form/table/sep_lista/label/nif"/></th></xsl:if><xsl:if test="not(rows/content/form/table/sep_lista/label/bi/@visible)"><th><xsl:value-of select="rows/content/form/table/sep_lista/label/bi"/></th></xsl:if><xsl:if test="not(rows/content/form/table/sep_lista/label/data_nasc/@visible)"><th><xsl:value-of select="rows/content/form/table/sep_lista/label/data_nasc"/></th></xsl:if><xsl:if test="not(rows/content/form/table/sep_lista/label/sexo/@visible)"><th><xsl:value-of select="rows/content/form/table/sep_lista/label/sexo"/></th></xsl:if><xsl:if test="not(rows/content/form/table/sep_lista/label/nome_pai/@visible)"><th><xsl:value-of select="rows/content/form/table/sep_lista/label/nome_pai"/></th></xsl:if><xsl:if test="not(rows/content/form/table/sep_lista/label/nome_mae/@visible)"><th><xsl:value-of select="rows/content/form/table/sep_lista/label/nome_mae"/></th></xsl:if><xsl:if test="not(rows/content/form/table/sep_lista/label/email/@visible)"><th><xsl:value-of select="rows/content/form/table/sep_lista/label/email"/></th></xsl:if><xsl:if test="not(rows/content/form/table/sep_lista/label/morada/@visible)"><th><xsl:value-of select="rows/content/form/table/sep_lista/label/morada"/></th></xsl:if><th align="center" width="10"></th><th align="center" width="10"></th></tr></thead><!--CORPO DA TABELA--><tbody><xsl:for-each select="rows/content/form/table/sep_lista/value/row"><tr><xsl:if test="not(nome/@visible)"><td><xsl:value-of select="nome_desc" /><input type="hidden" name="p_nome_fk" value="{nome}" /><input type="hidden" name="p_nome_fk_desc" value="{nome_desc}" /></td></xsl:if><xsl:if test="nome/@visible='false'"><input type="hidden" name="p_nome_fk" value="{nome}" /><input type="hidden" name="p_nome_fk_desc" value="{nome_desc}" /></xsl:if><xsl:if test="not(nif/@visible)"><td><xsl:value-of select="nif_desc" /><input type="hidden" name="p_nif_fk" value="{nif}" /><input type="hidden" name="p_nif_fk_desc" value="{nif_desc}" /></td></xsl:if><xsl:if test="nif/@visible='false'"><input type="hidden" name="p_nif_fk" value="{nif}" /><input type="hidden" name="p_nif_fk_desc" value="{nif_desc}" /></xsl:if><xsl:if test="not(bi/@visible)"><td><xsl:value-of select="bi_desc" /><input type="hidden" name="p_bi_fk" value="{bi}" /><input type="hidden" name="p_bi_fk_desc" value="{bi_desc}" /></td></xsl:if><xsl:if test="bi/@visible='false'"><input type="hidden" name="p_bi_fk" value="{bi}" /><input type="hidden" name="p_bi_fk_desc" value="{bi_desc}" /></xsl:if><xsl:if test="not(data_nasc/@visible)"><td><xsl:value-of select="data_nasc_desc" /><input type="hidden" name="p_data_nasc_fk" value="{data_nasc}" /><input type="hidden" name="p_data_nasc_fk_desc" value="{data_nasc_desc}" /></td></xsl:if><xsl:if test="data_nasc/@visible='false'"><input type="hidden" name="p_data_nasc_fk" value="{data_nasc}" /><input type="hidden" name="p_data_nasc_fk_desc" value="{data_nasc_desc}" /></xsl:if><xsl:if test="not(sexo/@visible)"><td><xsl:value-of select="sexo_desc" /><input type="hidden" name="p_sexo_fk" value="{sexo}" /><input type="hidden" name="p_sexo_fk_desc" value="{sexo_desc}" /></td></xsl:if><xsl:if test="sexo/@visible='false'"><input type="hidden" name="p_sexo_fk" value="{sexo}" /><input type="hidden" name="p_sexo_fk_desc" value="{sexo_desc}" /></xsl:if><xsl:if test="not(nome_pai/@visible)"><td><xsl:value-of select="nome_pai_desc" /><input type="hidden" name="p_nome_pai_fk" value="{nome_pai}" /><input type="hidden" name="p_nome_pai_fk_desc" value="{nome_pai_desc}" /></td></xsl:if><xsl:if test="nome_pai/@visible='false'"><input type="hidden" name="p_nome_pai_fk" value="{nome_pai}" /><input type="hidden" name="p_nome_pai_fk_desc" value="{nome_pai_desc}" /></xsl:if><xsl:if test="not(nome_mae/@visible)"><td><xsl:value-of select="nome_mae_desc" /><input type="hidden" name="p_nome_mae_fk" value="{nome_mae}" /><input type="hidden" name="p_nome_mae_fk_desc" value="{nome_mae_desc}" /></td></xsl:if><xsl:if test="nome_mae/@visible='false'"><input type="hidden" name="p_nome_mae_fk" value="{nome_mae}" /><input type="hidden" name="p_nome_mae_fk_desc" value="{nome_mae_desc}" /></xsl:if><xsl:if test="not(email/@visible)"><td><xsl:value-of select="email_desc" /><input type="hidden" name="p_email_fk" value="{email}" /><input type="hidden" name="p_email_fk_desc" value="{email_desc}" /></td></xsl:if><xsl:if test="email/@visible='false'"><input type="hidden" name="p_email_fk" value="{email}" /><input type="hidden" name="p_email_fk_desc" value="{email_desc}" /></xsl:if><xsl:if test="not(morada/@visible)"><td><xsl:value-of select="morada_desc" /><input type="hidden" name="p_morada_fk" value="{morada}" /><input type="hidden" name="p_morada_fk_desc" value="{morada_desc}" /></td></xsl:if><xsl:if test="morada/@visible='false'"><input type="hidden" name="p_morada_fk" value="{morada}" /><input type="hidden" name="p_morada_fk_desc" value="{morada_desc}" /></xsl:if><td align="center" width="10"><input type="hidden" name="p_sep_lista_id" value="{sep_lista_id}" /><xsl:if test="not(@nodelete)"><img alt="apagar" src="{$path}/img/icon/ctx-delete.png" class="IGRP_delRow" rel="sep_lista"/></xsl:if></td><td align="center" width="10"><xsl:if test="not(@noupdate)"><img alt="editar" src="{$path}/img/icon/edit.png" class="IGRP_editRow" rel="sep_lista"/></xsl:if></td></tr></xsl:for-each></tbody></table></div><!--FIM DA TABELA--></xsl:if></form></div><!--END FORM-->


