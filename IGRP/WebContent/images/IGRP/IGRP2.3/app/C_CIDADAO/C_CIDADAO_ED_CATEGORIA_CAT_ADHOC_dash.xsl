ste"><td class="table_row{$pos}" align="left"><xsl:value-of select="teste" /></td></xsl:if></tr></xsl:for-each></tbody></table></div><!--FIM DA TABELA--></form><!--END PAGE--></div><!-- RIGHT COLUMN --><div class="right-1"><!--GRAFICO--><div class="graph-box"><!-- PAGE SUBTITLE --><xsl:call-template name="page-title"><xsl:with-param name="title" select="rows/content/laranjeiro/caption" /><xsl:with-param name="class" select="'subtitle'" /></xsl:call-template><xsl:call-template name="igrp-graph"><xsl:with-param name="table" select="rows/content/laranjeiro" /><xsl:with-param name="chart_type" select="'LineChart'" /><xsl:with-param name="width" select="273" /><xsl:with-param name="height" select="150" /></xsl:call-template></div><!--GRAFICO--><div class="graph-box"><!-- PAGE SUBTITLE --><xsl:call-template name="page-title"><xsl:with-param name="title" select="rows/content/laranjeiro2/caption" /><xsl:with-param name="class" select="'subtitle'" /></xsl:call-template><xsl:call-template name="igrp-graph"><xsl:with-param name="table" select="rows/content/laranjeiro2" /><xsl:with-param name="chart_type" select="'PieChart'" /><xsl:with-param name="width" select="273" /><xsl:with-param name="height" select="150" /></xsl:call-template></div></div>


