		  <!-- CENTER COLUMN --><div class="center-1"><!--START PAGE--><!--VIEW --><!--START FORM VIEW--><div class="box-content"><div class="box-entity"><div class="foto-box"><img class="foto" src="{rows/content/view/value/view_foto}" /></div><div class="info-box"><ul><li><span class="label"><xsl:value-of select="rows/content/view/label/bi"/>:</span><xsl:value-of select="rows/content/view/value/bi"/></li><li><span class="label"><xsl:value-of select="rows/content/view/label/nome"/>:</span><xsl:value-of select="rows/content/view/value/nome"/></li><li><span class="label"><xsl:value-of select="rows/content/view/label/data_nasc"/>:</span><xsl:value-of select="rows/content/view/value/data_nasc"/></li></ul></div></div></div><!--END FORM VIEW--><!--SIMPLE-MENU--><xsl:apply-templates mode="menu-simple" select="rows/content/menu" /><form action="#" method="post" id="formular_default" name="formular_default" class="default_form"  enctype="multipart/form-data"><!--FORMULARIO --><!--START FORM--><!--TOOL BAR--><xsl:apply-templates mode="tools-bar" select="rows/content/form/tools-bar" /><div class="box-content"><!--HIDDEN FIELDS--><xsl:apply-templates mode="form-hidden" select="rows/content/form/value" /><xsl:if test="rows/content/form/label/nome_form"><label><span><xsl:value-of select="rows/content/form/label/nome_form"/></span><input type="text" name="{rows/content/form/value/nome_form/@name}" value="{rows/content/form/value/nome_form}"  class="" maxlength="150" /></label></xsl:if><xsl:if test="rows/content/form/label/idade_form"><label><span><xsl:value-of select="rows/content/form/label/idade_form"/></span><input type="text" name="{rows/content/form/value/idade_form/@name}" value="{rows/content/form/value/idade_form}"  class="" maxlength="30" /></label></xsl:if><xsl:if test="rows/content/form/label/lista_ad_hoc"><xsl:call-template name="page-title"><xsl:with-param name="title" select="rows/content/form/label/lista_ad_hoc" /><xsl:with-param name="class" select="'subtitle'" /></xsl:call-template></xsl:if><xsl:if test="rows/content/form/label/lista_ad_hoc"><!--INICIO DA TABELA--><div class="simple-table"><table rel="T_lista_ad_hoc"><!--CABECALHO DA TABELA--><thead><tr><th><xsl:value-of select="rows/content/form/table/lista_ad_hoc/label/bi_lista"/></th><th><xsl:value-of select="rows/content/form/table/lista_ad_hoc/label/nome_lista"/></th><th><xsl:value-of select="rows/content/form/table/lista_ad_hoc/label/data_lista"/></th></tr></thead><!--CORPO DA TABELA--><tbody><xsl:for-each select="rows/content/form/table/lista_ad_hoc/value/row"><xsl:variable name="pos" select="(position()+1) mod 2"/><tr class="IGRP_contextMenu"><td class="table_row{$pos}"><xsl:value-of select="bi_lista" /></td><td class="table_row{$pos}"><xsl:value-of select="nome_lista" /></td><td class="table_row{$pos}"><xsl:value-of select="data_lista" /></td></tr></xsl:for-each></tbody></table></div><!--FIM DA TABELA--></xsl:if><xsl:if test="rows/content/form/label/lista_fim"><xsl:call-template name="page-title"><xsl:with-param name="title" select="rows/content/form/label/lista_fim" /><xsl:with-param name="class" select="'subtitle'" /></xsl:call-template></xsl:if><xsl:if test="rows/content/form/label/data_de_hoje"><label><span><xsl:value-of select="rows/content/form/label/data_de_hoje"/></span><input type="date" name="{rows/content/form/value/data_de_hoje/@name}" value="{rows/content/form/value/data_de_hoje}" class="IGRP_datepicker date" /></label></xsl:if><xsl:if test="rows/content/form/label/google"><label><span><a href="{rows/content/form/value/google}" class="_blank"><xsl:value-of select="rows/content/form/label/google"/></a></span></label></xsl:if><xsl:if test="rows/content/form/label/separador_lista"><xsl:call-template name="page-title"><xsl:with-param name="title" select="rows/content/form/label/separador_lista" /><xsl:with-param name="class" select="'subtitle'" /></xsl:call-template></xsl:if><xsl:if test="rows/content/form/label/separador_lista"><xsl:call-template name="tools-bar-form-group"><xsl:with-param name="rel" select="'separador_lista'" /></xsl:call-template></xsl:if><xsl:if test="rows/content/form/label/nome_lista"><label><span><xsl:value-of select="rows/content/form/label/nome_lista"/></span><input type="text" name="{rows/content/form/value/nome_lista/@name}" value="{rows/content/form/value/nome_lista}"  rel="F_separador_lista"  class="" maxlength="30" /></label></xsl:if><xsl:if test="rows/content/form/label/bi_lista"><label><span><xsl:value-of select="rows/content/form/label/bi_lista"/></span><input type="number" name="{rows/content/form/value/bi_lista/@name}" value="{rows/content/form/value/bi_lista}"  rel="F_separador_lista"  class="number" maxlength="30" /></label></xsl:if><xsl:if test="rows/content/form/label/data_lista"><label><span><xsl:value-of select="rows/content/form/label/data_lista"/></span><input type="date" name="{rows/content/form/value/data_lista/@name}" value="{rows/content/form/value/data_lista}" class="IGRP_datepicker date"  rel="F_separador_lista" /></label></xsl:if><xsl:if test="rows/content/form/label/separador_lista"><!--INICIO DA TABELA--><div class="simple-table"><table rel="T_separador_lista"><!--CABECALHO DA TABELA--><thead><tr><th><xsl:value-of select="rows/content/form/table/separador_lista/label/nome_lista"/></th><th><xsl:value-of select="rows/content/form/table/separador_lista/label/bi_lista"/></th><th><xsl:value-of select="rows/content/form/table/separador_lista/label/data_lista"/></th><th align="center" width="10"></th><th align="center" width="10"></th></tr></thead><!--CORPO DA TABELA--><tbody><xsl:for-each select="rows/content/form/table/separador_lista/value/row"><tr><td><xsl:value-of select="nome_lista_desc" /><input type="hidden" name="p_separador_lista_id" value="{separador_lista_id}" /><input type="hidden" name="p_nome_lista_fk" value="{nome_lista}" /><input type="hidden" name="p_nome_lista_fk_desc" value="{nome_lista_desc}" /></td><td><xsl:value-of select="bi_lista_desc" /><input type="hidden" name="p_bi_lista_fk" value="{bi_lista}" /><input type="hidden" name="p_bi_lista_fk_desc" value="{bi_lista_desc}" /></td><td><xsl:value-of select="data_lista_desc" /><input type="hidden" name="p_data_lista_fk" value="{data_lista}" /><input type="hidden" name="p_data_lista_fk_desc" value="{data_lista_desc}" /></td><td align="center" width="10"><img alt="apagar" src="{$path}/img/icon/ctx-delete.png" class="IGRP_delRow" rel="separador_lista"/></td><td align="center" width="10"><img alt="editar" src="{$path}/img/icon/edit.png" class="IGRP_editRow" rel="separador_lista"/></td></tr></xsl:for-each></tbody></table></div><!--FIM DA TABELA--></xsl:if><xsl:if test="rows/content/form/label/fechar_separador"><xsl:call-template name="page-title"><xsl:with-param name="title" select="rows/content/form/label/fechar_separador" /><xsl:with-param name="class" select="'subtitle'" /></xsl:call-template></xsl:if></div><!--END FORM--><!--FILTRO --><!--START FORM--><div class="box-content"><!--HIDDEN FIELDS--><xsl:apply-templates mode="form-hidden" select="rows/content/filter/value" /><xsl:if test="rows/content/filter/label/bi"><label><span><xsl:value-of select="rows/content/filter/label/bi"/></span><input type="number" name="{rows/content/filter/value/bi/@name}" value="{rows/content/filter/value/bi}"  class="number" maxlength="10" /></label></xsl:if><xsl:if test="rows/content/filter/label/nome"><label><span><xsl:value-of select="rows/content/filter/label/nome"/></span><input type="text" name="{rows/content/filter/value/nome/@name}" value="{rows/content/filter/value/nome}"  class="" maxlength="50" /></label></xsl:if><!--TOOLBAR FILTER--><xsl:apply-templates mode="button-bar" select="rows/content/filter/tools-bar" /></div><!--END FORM--><!--TOOL BAR--><xsl:apply-templates mode="tools-bar" select="rows/content/table/tools-bar" /><!-- CONTEXT-MENU --><xsl:apply-templates mode="context-menu" select="rows/content/table/context-menu"/><!--INICIO DA TABELA--><div class="table"><table class="sortable IGRP_table"><!--CABECALHO DA TABELA--><thead><tr><xsl:if test="rows/content/table/label/bi"><th><xsl:value-of select="rows/content/table/label/bi"/></th></xsl:if><xsl:if test="rows/content/table/label/nome"><th><xsl:value-of select="rows/content/table/label/nome"/></th></xsl:if><xsl:if test="rows/content/table/label/data"><th><xsl:value-of select="rows/content/table/label/data"/></th></xsl:if><xsl:if test="rows/content/table/label/eliminar"><th class="checkbox sorttable_nosort" align="center"><input type="checkbox" name="p_eliminar_all" value="{value}" class="IGRP_checkall" /></th></xsl:if></tr></thead><!--CORPO DA TABELA--><tbody><xsl:for-each select="rows/content/table/value/row"><xsl:variable name="pos" select="(position()+1) mod 2"/><tr class="IGRP_contextMenu"><xsl:apply-templates mode="context-param" select="context-menu" /><xsl:if test="bi"><td class="table_row{$pos}"><xsl:value-of select="bi" /></td></xsl:if><xsl:if test="nome"><td class="table_row{$pos}"><xsl:value-of select="nome" /></td></xsl:if><xsl:if test="data"><td class="table_row{$pos}"><xsl:value-of select="data" /></td></xsl:if><xsl:if test="eliminar"><td class="table_row{$pos} checkbox"><input type="checkbox" name="p_eliminar" value="{eliminar}"><xsl:if test="eliminar_check=eliminar"><xsl:attribute name="checked">checked</xsl:attribute></xsl:if></input><xsl:value-of select="eliminar_desc" /></td></xsl:if></tr></xsl:for-each></tbody></table></div><!--FIM DA TABELA--></form><!--END PAGE--></div><!-- RIGHT COLUMN --><div class="right-1"><xsl:if test="rows/content/test"><!--GRAFICO--><div class="graph-box"><xsl:call-template name="page-title"><xsl:with-param name="title" select="rows/content/test/caption" /><xsl:with-param name="class" select="'subtitle'" /></xsl:call-template><xsl:call-template name="igrp-graph"><xsl:with-param name="table" select="rows/content/test" /><xsl:with-param name="chart_type" select="'ColumnChart'" /><xsl:with-param name="width" select="273" /><xsl:with-param name="height" select="150" /></xsl:call-template></div></xsl:if></div>


