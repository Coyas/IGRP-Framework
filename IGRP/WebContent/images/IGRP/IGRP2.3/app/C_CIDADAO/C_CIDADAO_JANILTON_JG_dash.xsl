l></xsl:if><xsl:if test="rows/content/form/label/lista_ah_doc"><xsl:call-template name="page-title"><xsl:with-param name="title" select="rows/content/form/label/lista_ah_doc" /><xsl:with-param name="class" select="'subtitle'" /></xsl:call-template></xsl:if><xsl:if test="rows/content/form/label/lista_ah_doc"><!--INICIO DA TABELA--><div class="simple-table"><table rel="T_lista_ah_doc"><!--CABECALHO DA TABELA--><thead><tr><th><xsl:value-of select="rows/content/form/table/lista_ah_doc/label/bi_lista"/></th><th><xsl:value-of select="rows/content/form/table/lista_ah_doc/label/nome_lista"/></th><th><xsl:value-of select="rows/content/form/table/lista_ah_doc/label/data_lista"/></th></tr></thead><!--CORPO DA TABELA--><tbody><xsl:for-each select="rows/content/form/table/lista_ah_doc/value/row"><xsl:variable name="pos" select="(position()+1) mod 2"/><tr class="IGRP_contextMenu"><td class="table_row{$pos}"><xsl:value-of select="bi_lista" /></td><td class="table_row{$pos}"><xsl:value-of select="nome_lista" /></td><td class="table_row{$pos}"><xsl:value-of select="data_lista" /></td></tr></xsl:for-each></tbody></table></div><!--FIM DA TABELA--></xsl:if><xsl:if test="rows/content/form/label/lista_fim"><xsl:call-template name="page-title"><xsl:with-param name="title" select="rows/content/form/label/lista_fim" /><xsl:with-param name="class" select="'subtitle'" /></xsl:call-template></xsl:if><xsl:if test="rows/content/form/label/data_hj"><label><span><xsl:value-of select="rows/content/form/label/data_hj"/></span><input type="date" name="{rows/content/form/value/data_hj/@name}" value="{rows/content/form/value/data_hj}" class="IGRP_datepicker date" /></label></xsl:if><xsl:if test="rows/content/form/label/separador_lista"><xsl:call-template name="page-title"><xsl:with-param name="title" select="rows/content/form/label/separador_lista" /><xsl:with-param name="class" select="'subtitle'" /></xsl:call-template></xsl:if><xsl:if test="rows/content/form/label/separador_lista"><xsl:call-template name="tools-bar-form-group"><xsl:with-param name="rel" select="'separador_lista'" /></xsl:call-template></xsl:if><xsl:if test="rows/content/form/label/sep_nome"><label><span><xsl:value-of select="rows/content/form/label/sep_nome"/></span><input type="text" name="{rows/content/form/value/sep_nome/@name}" value="{rows/content/form/value/sep_nome}"  rel="F_separador_lista"  class="" maxlength="150" /></label></xsl:if><xsl:if test="rows/content/form/label/sep_bi"><label><span><xsl:value-of select="rows/content/form/label/sep_bi"/></span><input type="text" name="{rows/content/form/value/sep_bi/@name}" value="{rows/content/form/value/sep_bi}"  rel="F_separador_lista"  class="" maxlength="30" /><xsl:call-template name="lookup-tool"><xsl:with-param name="page" select="'LOOKUP'" /><xsl:with-param name="action" select="'LOOKUP_BI'" /><xsl:with-param name="name" select="rows/content/form/value/sep_bi/@name" /><xsl:with-param name="js_lookup" select="rows/content/form/lookup/sep_bi" /></xsl:call-template></label></xsl:if><xsl:if test="rows/content/form/label/sep_data"><label><span><xsl:value-of select="rows/content/form/label/sep_data"/></span><input type="date" name="{rows/content/form/value/sep_data/@name}" value="{rows/content/form/value/sep_data}" class="IGRP_datepicker date"  rel="F_separador_lista" /></label></xsl:if><xsl:if test="rows/content/form/label/separador_lista"><!--INICIO DA TABELA--><div class="simple-table"><table rel="T_separador_lista"><!--CABECALHO DA TABELA--><thead><tr><th><xsl:value-of select="rows/content/form/table/separador_lista/label/sep_nome"/></th><th><xsl:value-of select="rows/content/form/table/separador_lista/label/sep_bi"/></th><th><xsl:value-of select="rows/content/form/table/separador_lista/label/sep_data"/></th><th align="center" width="10"></th><th align="center" width="10"></th></tr></thead><!--CORPO DA TABELA--><tbody><xsl:for-each select="rows/content/form/table/separador_lista/value/row"><tr><td><xsl:value-of select="sep_nome_desc" /><input type="hidden" name="p_separador_lista_id" value="{separador_lista_id}" /><input type="hidden" name="p_sep_nome_fk" value="{sep_nome}" /><input type="hidden" name="p_sep_nome_fk_desc" value="{sep_nome_desc}" /></td><td><xsl:value-of select="sep_bi_desc" /><input type="hidden" name="p_sep_bi_fk" value="{sep_bi}" /><input type="hidden" name="p_sep_bi_fk_desc" value="{sep_bi_desc}" /></td><td><xsl:value-of select="sep_data_desc" /><input type="hidden" name="p_sep_data_fk" value="{sep_data}" /><input type="hidden" name="p_sep_data_fk_desc" value="{sep_data_desc}" /></td><td align="center" width="10"><img alt="apagar" src="{$path}/img/icon/ctx-delete.png" class="IGRP_delRow" rel="separador_lista"/></td><td align="center" width="10"><img alt="editar" src="{$path}/img/icon/edit.png" class="IGRP_editRow" rel="separador_lista"/></td></tr></xsl:for-each></tbody></table></div><!--FIM DA TABELA--></xsl:if></div><!--END FORM--><!--FILTRO --><!--START FORM--><div class="box-content"><!--HIDDEN FIELDS--><xsl:apply-templates mode="form-hidden" select="rows/content/filter/value" /><xsl:if test="rows/content/filter/label/bi"><label><span><xsl:value-of select="rows/content/filter/label/bi"/></span><input type="text" name="{rows/content/filter/value/bi/@name}" value="{rows/content/filter/value/bi}"  class="" maxlength="30" /><xsl:call-template name="lookup-tool"><xsl:with-param name="page" select="'LOOKUP'" /><xsl:with-param name="action" select="'LOOKUP_BI'" /><xsl:with-param name="name" select="rows/content/filter/value/bi/@name" /><xsl:with-param name="js_lookup" select="rows/content/filter/lookup/bi" /></xsl:call-template></label></xsl:if><xsl:if test="rows/content/filter/label/nome"><label><span><xsl:value-of select="rows/content/filter/label/nome"/></span><input type="text" name="{rows/content/filter/value/nome/@name}" value="{rows/content/filter/value/nome}"  class="" maxlength="150" /></label></xsl:if><!--TOOLBAR FILTER--><xsl:apply-templates mode="button-bar" select="rows/content/filter/tools-bar" /></div><!--END FORM--><!--TOOL BAR--><xsl:apply-templates mode="tools-bar" select="rows/content/table/tools-bar" /><!-- CONTEXT-MENU --><xsl:apply-templates mode="context-menu" select="rows/content/table/context-menu"/><!--INICIO DA TABELA--><div class="table"><table class="sortable IGRP_table"><!--CABECALHO DA TABELA--><thead><tr><xsl:if test="rows/content/table/label/bi"><th><xsl:value-of select="rows/content/table/label/bi"/></th></xsl:if><xsl:if test="rows/content/table/label/nome"><th><xsl:value-of select="rows/content/table/label/nome"/></th></xsl:if><xsl:if test="rows/content/table/label/data"><th><xsl:value-of select="rows/content/table/label/data"/></th></xsl:if><xsl:if test="rows/content/table/label/eliminar"><th class="checkbox sorttable_nosort" align="center"><input type="checkbox" name="p_eliminar_all" value="{value}" class="IGRP_checkall" /></th></xsl:if></tr></thead><!--CORPO DA TABELA--><tbody><xsl:for-each select="rows/content/table/value/row"><xsl:variable name="pos" select="(position()+1) mod 2"/><tr class="IGRP_contextMenu"><xsl:apply-templates mode="context-param" select="context-menu" /><xsl:if test="bi"><td class="table_row{$pos}"><xsl:value-of select="bi" /></td></xsl:if><xsl:if test="nome"><td class="table_row{$pos}"><xsl:value-of select="nome" /></td></xsl:if><xsl:if test="data"><td class="table_row{$pos}"><xsl:value-of select="data" /></td></xsl:if><xsl:if test="eliminar"><td class="table_row{$pos} checkbox"><input type="checkbox" name="p_eliminar" value="{eliminar}"><xsl:if test="eliminar_check=eliminar"><xsl:attribute name="checked">checked</xsl:attribute></xsl:if></input><xsl:value-of select="eliminar_desc" /></td></xsl:if></tr></xsl:for-each></tbody></table></div><!--FIM DA TABELA--></form><!--END PAGE--></div><!-- RIGHT COLUMN --><div class="right-1"><xsl:if test="rows/content/tst"><!--GRAFICO--><div class="graph-box"><xsl:call-template name="page-title"><xsl:with-param name="title" select="rows/content/tst/caption" /><xsl:with-param name="class" select="'subtitle'" /></xsl:call-template><xsl:call-template name="igrp-graph"><xsl:with-param name="table" select="rows/content/tst" /><xsl:with-param name="chart_type" select="'BarChart'" /><xsl:with-param name="width" select="273" /><xsl:with-param name="height" select="150" /></xsl:call-template></div></xsl:if></div>


