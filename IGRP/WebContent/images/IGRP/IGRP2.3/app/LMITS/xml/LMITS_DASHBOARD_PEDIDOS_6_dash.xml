    <content>
        <title>Cobranças no Mapa</title>
        <page_title type="sectionheader" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <page_title_text type="text" name="p_page_title_text" persist="true" maxlength="4000">
                    <value>
                        <![CDATA[Dashboard Cobranças]]>
                    </value>
                </page_title_text>
            </fields>
        </page_title>
        <table_1 type="table" xml-type="table" title="Atos" gen-type="container" gen-group="true">
            <fields>
                <tipo_cobranca name="p_tipo_cobranca" type="link" target="_self" class="primary" iconColor="#333" iconClass="" img="fa-link" maxlength="4000" align="left" lookup_parser="false" desc="true">
                    <label>Tipo Cobrança</label>
                </tipo_cobranca>
                <total name="p_total" type="text" maxlength="30" align="left" lookup_parser="false">
                    <label>Total</label>
                </total>
            </fields>
            <table>
                <value>
                    <row>
                        <context-menu>
                            <param>p1=linha1</param>
                            <param>p2=linha1</param>
                        </context-menu>
                        <tipo_cobranca name="p_tipo_cobranca" desc="true">http://dobwogfi.edu/gerehibi</tipo_cobranca>
                        <tipo_cobranca_desc name="p_tipo_cobranca_desc">http://dobwogfi.edu/gerehibi</tipo_cobranca_desc>
                        <total name="p_total">Rem lorem amet rem ut</total>
                    </row>
                    <row>
                        <context-menu>
                            <param>p1=linha1</param>
                            <param>p2=linha1</param>
                        </context-menu>
                        <tipo_cobranca name="p_tipo_cobranca" desc="true">http://ekana.co.uk/kid</tipo_cobranca>
                        <tipo_cobranca_desc name="p_tipo_cobranca_desc">http://ekana.co.uk/kid</tipo_cobranca_desc>
                        <total name="p_total">Officia sed doloremque doloremque unde</total>
                    </row>
                    <row>
                        <context-menu>
                            <param>p1=linha1</param>
                            <param>p2=linha1</param>
                        </context-menu>
                        <tipo_cobranca name="p_tipo_cobranca" desc="true">http://husuw.io/vizfoh</tipo_cobranca>
                        <tipo_cobranca_desc name="p_tipo_cobranca_desc">http://husuw.io/vizfoh</tipo_cobranca_desc>
                        <total name="p_total">Ut natus aliqua sit rem</total>
                    </row>
                    <row>
                        <context-menu>
                            <param>p1=linha1</param>
                            <param>p2=linha1</param>
                        </context-menu>
                        <tipo_cobranca name="p_tipo_cobranca" desc="true">http://jol.com/wif</tipo_cobranca>
                        <tipo_cobranca_desc name="p_tipo_cobranca_desc">http://jol.com/wif</tipo_cobranca_desc>
                        <total name="p_total">Accusantium rem adipiscing ut totam</total>
                    </row>
                </value>
                <context-menu>
                    <item type="specific" code="" class="success" rel="b_pago">
