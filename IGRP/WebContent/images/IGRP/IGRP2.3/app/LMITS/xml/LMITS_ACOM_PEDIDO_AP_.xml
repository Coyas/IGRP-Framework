<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet href="../LMITS_ACOM_PEDIDO_AP_.xsl" type="text/xsl"?>
<rows>
    <site>
        <welcome_note>Ola</welcome_note>
        <footer_note>2011 - Copyright NOSI</footer_note>
        <user_name>red-igrp</user_name>
        <button type="specific">
            <title>Sair</title>
            <app>RED</app>
            <page />
            <link>#</link>
            <target>_self</target>
            <img>exit.png</img>
        </button>
    </site>
    <page>ACOM_PEDIDO</page>
    <app>LMITS</app>
    <plsql>
        <action>AP</action>
        <package_instance></package_instance>
        <package_db>lmits_alter_process_db</package_db>
        <package_html>lmits_alter_process_html</package_html>
        <table_name></table_name>
        <with_replace>true</with_replace>
        <with_label>true</with_label>
        <with_biztalk>false</with_biztalk>
        <dynamic_menu>false</dynamic_menu>
        <copy_menu>false</copy_menu>
        <package_copy_db />
        <package_copy_html />
        <preserve_url>https://nosiappsdev.gov.cv/redglobal_dev/red.form_designer_db.preserve_your_code</preserve_url>
    </plsql>
    <content>
        <title>Alterar Processo</title>
        <page_title type="sectionheader" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <page_title_text type="text" name="p_page_title_text" persist="true" maxlength="4000">
                    <value>
                        <![CDATA[Alterar Processo]]>
                    </value>
                </page_title_text>
            </fields>
        </page_title>
        <box_1 type="box" xml-type="form" title="Informações do Processo" gen-type="container" gen-group="">
            <fields/>
        </box_1>
        <view_1 type="view" xml-type="view" gen-type="container" gen-group="">
            <fields>
                <numero_de_pedido name="p_numero_de_pedido" type="text" maxlength="30">
                    <label>Número de Pedido</label>
                    <value>Natus consectetur officia deserunt consectetur</value>
                </numero_de_pedido>
                <id_det_pedido name="p_id_det_pedido" type="text" maxlength="30">
                    <label>Número de Processo</label>
                    <value>Sit unde deserunt ipsum officia</value>
                </id_det_pedido>
                <tipo__de_pedido name="p_tipo__de_pedido" type="text" maxlength="200">
                    <label>Tipo  de Pedido</label>
                    <value>Consectetur magna amet labore laudantium</value>
                </tipo__de_pedido>
            </fields>
        </view_1>
        <form_1 type="form" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <atencao name="p_atencao" type="plaintext" clear="false" maxlength="30" right="false">
                    <label>Atencao</label>
                    <value>Magna officia rem aliqua natus sed iste voluptatem sed doloremque ipsum deserunt totam sed iste</value>
                </atencao>
                <ato_de name="p_ato_de" type="text" maxlength="200" required="true" change="false" readonly="false" disabled="true" placeholder="" right="false">
                    <label>Ato (De)</label>
                    <value>Sit deserunt iste rem mollit</value>
                </ato_de>
                <ato_para name="p_ato_para" type="select" multiple="false" maxlength="200" required="true" change="false" disabled="false" right="false">
                    <label>Ato (Para)</label>
                    <list>
                        <option>
                            <text>Option 1</text>
                            <value>1</value>
                        </option>
                        <option>
                            <text>Option 2</text>
                            <value>2</value>
                        </option>
                        <option>
                            <text>Option 3</text>
                            <value>3</value>
                        </option>
                        <option>
                            <text>Option 4</text>
                            <value>4</value>
                        </option>
                    </list>
                </ato_para>
                <notas name="p_notas" type="textarea" maxlength="4000" required="true" change="false" readonly="false" disabled="false" placeholder="" right="false">
                    <label>Notas</label>
                    <value>Dolor laudantium lorem consectetur perspiciatis iste sed iste mollit elit lorem adipiscing elit natus aliqua</value>
                </notas>
            </fields>
            <tools-bar>
                <item type="form" code="" flg_transaction="true" class="danger" rel="mudar_processo">
                    <title>Mudar Processo</title>
                    <app>LMITS</app>
                    <page>ACOM_PEDIDO</page>
                    <link>AP</link>
                    <target>submit</target>
                    <img>danger|fa-gavel</img>
                </item>
            </tools-bar>
        </form_1>
        <view_2 type="view" xml-type="view" gen-type="container" gen-group="">
            <fields>
                <predio name="p_predio" type="text" maxlength="30">
                    <label>Prédio</label>
                    <value>Sed consectetur perspiciatis accusantium sit</value>
                </predio>
                <data_de_pedido name="p_data_de_pedido" type="text" maxlength="30">
                    <label>Data de Pedido</label>
                    <value>Voluptatem aliqua rem deserunt unde</value>
                </data_de_pedido>
                <estado name="p_estado" type="text" maxlength="100">
                    <label>Estado</label>
                    <value>Sed laudantium mollit adipiscing dolor</value>
                </estado>
            </fields>
        </view_2>
    </content>
</rows>