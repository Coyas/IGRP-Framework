    <content>
        <title>Dashboard Dívida</title>
        <form_1 type="form" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <metrica name="p_metrica" type="select" multiple="false" maxlength="30" required="false" change="false" disabled="false" right="false">
                    <label>Métrica</label>
                    <list>
                        <option>
                            <text>Option 1</text>
                            <value>1</value>
                        </option>
                        <option>
                            <text>Option 2</text>
                            <value>2</value>
                        </option>
                        <option>
                            <text>Option 3</text>
                            <value>3</value>
                        </option>
                        <option>
                            <text>Option 4</text>
                            <value>4</value>
                        </option>
                    </list>
                </metrica>
                <ano name="p_ano" type="text" maxlength="30" required="false" change="false" readonly="false" disabled="false" placeholder="" right="false">
                    <label>Ano</label>
                    <value>Ipsum magna labore dolor anim</value>
                </ano>
                <tipo_predio name="p_tipo_predio" type="select" multiple="false" maxlength="30" required="false" change="false" disabled="false" right="false">
                    <label>Tipo Prédio</label>
                    <list>
                        <option>
                            <text>Option 1</text>
                            <value>1</value>
                        </option>
                        <option>
                            <text>Option 2</text>
                            <value>2</value>
                        </option>
                        <option>
                            <text>Option 3</text>
                            <value>3</value>
                        </option>
                        <option>
                            <text>Option 4</text>
                            <value>4</value>
                        </option>
                    </list>
                </tipo_predio>
                <tipo_cobranca name="p_tipo_cobranca" type="select" multiple="false" maxlength="30" required="false" change="false" disabled="false" right="false">
                    <label>Tipo Cobrança</label>
                    <list>
                        <option>
                            <text>Option 1</text>
                            <value>1</value>
                        </option>
                        <option>
                            <text>Option 2</text>
                            <value>2</value>
                        </option>
                        <option>
                            <text>Option 3</text>
                            <value>3</value>
                        </option>
                        <option>
                            <text>Option 4</text>
                            <value>4</value>
                        </option>
                    </list>
                </tipo_cobranca>
                <estado_divida name="p_estado_divida" type="select" multiple="false" maxlength="30" required="false" change="false" disabled="false" right="false">
                    <label>Estado Dívida</label>
                    <list>
                        <option>
                            <text>Option 1</text>
                            <value>1</value>
                        </option>
                        <option>
                            <text>Option 2</text>
                            <value>2</value>
                        </option>
                        <option>
                            <text>Option 3</text>
                            <value>3</value>
                        </option>
                        <option>
                            <text>Option 4</text>
                            <value>4</value>
                        </option>
                    </list>
                </estado_divida>
                <data_de name="p_data_de" type="date" format="IGRP_datePicker" maxlength="30" required="false" change="false" readonly="false" disabled="false" placeholder="" right="false" class="default">
                    <label>Data de</label>
                    <value/>
                </data_de>
                <data_ate name="p_data_ate" type="date" format="IGRP_datePicker" maxlength="30" required="false" change="false" readonly="false" disabled="false" placeholder="" right="false" class="default">
                    <label>Data Até</label>
                    <value/>
                </data_ate>
                <zona name="p_zona" type="lookup" action="DASH_DIV" page="DASH" app="LMITS" class="default" maxlength="30" required="false" change="false" readonly="false" disabled="false" placeholder="" right="false">
                    <label>Zona</label>
                    <value/>
                    <lookup>http://igrp.teste.gov.cv/images/IGRP.bootstrap/app/BOOTSTRAP/xml/lookup.test.xml</lookup>
                </zona>
                <utente name="p_utente" type="text" maxlength="300" required="false" change="false" readonly="false" disabled="false" placeholder="" right="false">
                    <label>Utente</label>
                    <value>Elit amet omnis labore ipsum</value>
                </utente>
                <proprietario name="p_proprietario" type="text" maxlength="300" required="false" change="false" readonly="false" disabled="false" placeholder="" right="false">
                    <label>Proprietário</label>
                    <value>Sit dolor elit doloremque magna</value>
                </proprietario>
                <matriz name="p_matriz" type="lookup" action="DASH_DIV" page="DASH" app="LMITS" class="default" maxlength="30" required="false" change="false" readonly="false" disabled="false" placeholder="" right="false">
                    <label>Matriz</label>
                    <value/>
                    <lookup>http://igrp.teste.gov.cv/images/IGRP.bootstrap/app/BOOTSTRAP/xml/lookup.test.xml</lookup>
                </matriz>
                <nip name="p_nip" type="lookup" action="DASH_DIV" page="DASH" app="LMITS" class="default" maxlength="30" required="false" change="false" readonly="false" disabled="false" placeholder="" right="false">
                    <label>NIP</label>
                    <value/>
                    <lookup>http://igrp.teste.gov.cv/images/IGRP.bootstrap/app/BOOTSTRAP/xml/lookup.test.xml</lookup>
                </nip>
                <hidden name="p_id_matriz" type="hidden" maxlength="30" tag="id_matriz"/>
                <hidden name="p_ba_unit_id" type="hidden" maxlength="30" tag="ba_unit_id"/>
                <hidden name="p_nip_id" type="hidden" maxlength="30" tag="nip_id"/>
                <hidden name="p_zona_id" type="hidden" maxlength="30" tag="zona_id"/>
            </fields>
            <tools-bar>
                <item type="form" code="" class="primary" rel="pesq">
                    <title>Pesquisar</title>
                    <app>LMITS</app>
                    <page>DASH</page>
                    <link>DASH_DIV</link>
                    <target>submit</target>
                    <img>primary|fa-angle-right</img>
                </item>
            </tools-bar>
        </form_1>
