    <content>
        <title>Pedidos No Mapa</title>
        <page_title type="sectionheader" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <page_title_text type="text" name="p_page_title_text" persist="true" maxlength="100">
                    <value>
                        <![CDATA[Pedidos de Atos - Visão Espacial]]>
                    </value>
                </page_title_text>
            </fields>
        </page_title>
        <table_1 type="table" xml-type="table" title="Atos" gen-type="container" gen-group="true">
            <fields>
                <atos name="p_atos" type="link" target="_self" class="primary" iconColor="#333" iconClass="" img="fa-link" maxlength="4000" align="left" lookup_parser="false" desc="true">
                    <label>Atos</label>
                </atos>
            </fields>
            <table>
                <value>
                    <row>
                        <context-menu>
                            <param>p1=linha1</param>
                            <param>p2=linha1</param>
                        </context-menu>
                        <atos name="p_atos" desc="true">http://zu.edu/akja</atos>
                        <atos_desc name="p_atos_desc">http://zu.edu/akja</atos_desc>
                    </row>
                    <row>
                        <context-menu>
                            <param>p1=linha1</param>
                            <param>p2=linha1</param>
                        </context-menu>
                        <atos name="p_atos" desc="true">http://he.edu/tupapi</atos>
                        <atos_desc name="p_atos_desc">http://he.edu/tupapi</atos_desc>
                    </row>
                    <row>
                        <context-menu>
                            <param>p1=linha1</param>
                            <param>p2=linha1</param>
                        </context-menu>
                        <atos name="p_atos" desc="true">http://visrah.com/evmi</atos>
                        <atos_desc name="p_atos_desc">http://visrah.com/evmi</atos_desc>
                    </row>
                    <row>
                        <context-menu>
                            <param>p1=linha1</param>
                            <param>p2=linha1</param>
                        </context-menu>
                        <atos name="p_atos" desc="true">http://tiodra.io/jowikog</atos>
                        <atos_desc name="p_atos_desc">http://tiodra.io/jowikog</atos_desc>
                    </row>
                </value>
                <context-menu>
                    <item type="specific" code="" class="warning" rel="b_curso">
                        <title>Em curso</title>
                        <app>LMITS</app>
                        <page>DASHBOARD_PEDIDOS</page>
                        <link>5</link>
                        <target>_self</target>
                        <img>warning|fa-spinner</img>
                    </item>
                    <item type="specific" code="" class="success" rel="b_end">
                        <title>Terminado</title>
                        <app>LMITS</app>
                        <page>DASHBOARD_PEDIDOS</page>
                        <link>5</link>
                        <target>_self</target>
                        <img>success|fa-delicious</img>
                    </item>
                </context-menu>
            </table>
        </table_1>
        <mapchart_1 type="mapchart" xml-type="form" title="Zonas com Pedidos" gen-type="container" gen-group="">
            <fields>
                <mapchart_1_basemap name="p_mapchart_1_basemap" type="text" maxlength="300">
                    <label>Basemap</label>
                    <value>https://nosiappsdev.gov.cv/images/IGRP/IGRP2.3/utils/geo/basemap/caboverde/json.geojson</value>
                </mapchart_1_basemap>
                <mapchart_1_data name="p_mapchart_1_data" type="text" maxlength="300">
                    <label>Data</label>
                    <value>https://nosiappsdev.gov.cv/images/IGRP/IGRP2.3/utils/geo/data/data.json</value>
                </mapchart_1_data>
                <mapchart_1_config name="p_mapchart_1_config" type="text" maxlength="300">
                    <label>Configuration</label>
                    <value>https://nosiappsdev.gov.cv/images/IGRP/IGRP2.3/utils/geo/config/config2.json</value>
                </mapchart_1_config>
            </fields>
