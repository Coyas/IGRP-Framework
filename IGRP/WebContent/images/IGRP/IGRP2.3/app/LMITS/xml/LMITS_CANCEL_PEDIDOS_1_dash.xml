                    <row>
                        <selccionar name="p_selccionar" check="true" desc="true">1</selccionar>
                        <selccionar_desc name="p_selccionar_desc">1</selccionar_desc>
                        <accao name="p_accao">Anim natus consectetur adipiscing natus</accao>
                    </row>
                    <row>
                        <selccionar name="p_selccionar" check="true" desc="true">1</selccionar>
                        <selccionar_desc name="p_selccionar_desc">1</selccionar_desc>
                        <accao name="p_accao">Accusantium anim deserunt amet aperiam</accao>
                    </row>
                </value>
            </table>
        </table_1>
        <table_2 type="table" xml-type="table" title="Consequências de Cancelamento" gen-type="container" gen-group="true">
            <fields>
                <gravidade name="p_gravidade" type="text" maxlength="30" align="left" lookup_parser="false">
                    <label>Gravidade</label>
                </gravidade>
                <consequencia name="p_consequencia" type="text" maxlength="200" align="left" lookup_parser="false">
                    <label>Consequência</label>
                </consequencia>
            </fields>
            <table>
                <value>
                    <row>
                        <gravidade name="p_gravidade">Elit rem sit officia sit</gravidade>
                        <consequencia name="p_consequencia">Totam sed voluptatem rem magna</consequencia>
                    </row>
                    <row>
                        <gravidade name="p_gravidade">Sed consectetur lorem perspiciatis mollit</gravidade>
