            <fields>
                <selccionar name="p_selccionar" type="checkbox" maxlength="30" align="left" lookup_parser="false" check="true" desc="true">
                    <label>Selccionar</label>
                </selccionar>
                <accao name="p_accao" type="text" maxlength="30" align="left" lookup_parser="false">
                    <label>Acção</label>
                </accao>
            </fields>
            <table>
                <value>
                    <row>
                        <selccionar name="p_selccionar" check="true" desc="true">1</selccionar>
                        <selccionar_desc name="p_selccionar_desc">1</selccionar_desc>
                        <accao name="p_accao">Omnis stract sed accusantium laudantium</accao>
                    </row>
                    <row>
                        <selccionar name="p_selccionar" check="true" desc="true">1</selccionar>
                        <selccionar_desc name="p_selccionar_desc">1</selccionar_desc>
