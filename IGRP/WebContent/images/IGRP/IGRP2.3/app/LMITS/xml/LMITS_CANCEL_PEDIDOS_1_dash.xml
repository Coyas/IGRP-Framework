                    <value/>
                </data_de_pedido>
                <id_pedido name="p_id_pedido" type="hidden" maxlength="30" tag="id_pedido"/>
                <view_img maxlength="300">http://igrp.teste.gov.cv/images/legislativas/data/img/candidatos/jon_doe.jpg</view_img>
