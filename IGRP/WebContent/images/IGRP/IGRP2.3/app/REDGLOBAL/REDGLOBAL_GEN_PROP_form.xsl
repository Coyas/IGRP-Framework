ontent/form/table/valdation/label/procedure"/></th></xsl:if></xsl:if><th align="center" width="10"></th><th align="center" width="10"></th></tr></thead><!--CORPO DA TABELA--><tbody><xsl:for-each select="rows/content/form/table/valdation/value/row[not(@total='yes')]"><tr><xsl:if test="description"><xsl:if test="not(description/@visible)"><td><xsl:value-of select="description_desc" /><input type="hidden" name="p_description_fk" value="{description}" /><input type="hidden" name="p_description_fk_desc" value="{description_desc}" /></td></xsl:if></xsl:if><xsl:if test="description/@visible='false'"><input type="hidden" name="p_description_fk" value="{description}" /><input type="hidden" name="p_description_fk_desc" value="{description_desc}" /></xsl:if><xsl:if test="event"><xsl:if test="not(event/@visible)"><td><xsl:value-of select="event_desc" /><input type="hidden" name="p_event_fk" value="{event}" /><input type="hidden" name="p_event_fk_desc" value="{event_desc}" /></td></xsl:if></xsl:if><xsl:if test="event/@visible='false'"><input type="hidden" name="p_event_fk" value="{event}" /><input type="hidden" name="p_event_fk_desc" value="{event_desc}" /></xsl:if><xsl:if test="condition"><xsl:if test="not(condition/@visible)"><td><xsl:value-of select="condition_desc" /><input type="hidden" name="p_condition_fk" value="{condition}" /><input type="hidden" name="p_condition_fk_desc" value="{condition_desc}" /></td></xsl:if></xsl:if><xsl:if test="condition/@visible='false'"><input type="hidden" name="p_condition_fk" value="{condition}" /><input type="hidden" name="p_condition_fk_desc" value="{condition_desc}" /></xsl:if><xsl:if test="value"><xsl:if test="not(value/@visible)"><td><xsl:value-of select="value_desc" /><input type="hidden" name="p_value_fk" value="{value}" /><input type="hidden" name="p_value_fk_desc" value="{value_desc}" /></td></xsl:if></xsl:if><xsl:if test="value/@visible='false'"><input type="hidden" name="p_value_fk" value="{value}" /><input type="hidden" name="p_value_fk_desc" value="{value_desc}" /></xsl:if><xsl:if test="action"><xsl:if test="not(action/@visible)"><td><xsl:value-of select="action_desc" /><input type="hidden" name="p_action_fk" value="{action}" /><input type="hidden" name="p_action_fk_desc" value="{action_desc}" /></td></xsl:if></xsl:if><xsl:if test="action/@visible='false'"><input type="hidden" name="p_action_fk" value="{action}" /><input type="hidden" name="p_action_fk_desc" value="{action_desc}" /></xsl:if><xsl:if test="target"><xsl:if test="not(target/@visible)"><td><xsl:value-of select="target_desc" /><input type="hidden" name="p_target_fk" value="{target}" /><input type="hidden" name="p_target_fk_desc" value="{target_desc}" /></td></xsl:if></xsl:if><xsl:if test="target/@visible='false'"><input type="hidden" name="p_target_fk" value="{target}" /><input type="hidden" name="p_target_fk_desc" value="{target_desc}" /></xsl:if><xsl:if test="procedure"><xsl:if test="not(procedure/@visible)"><td><xsl:value-of select="procedure_desc" /><input type="hidden" name="p_procedure_fk" value="{procedure}" /><input type="hidden" name="p_procedure_fk_desc" value="{procedure_desc}" /></td></xsl:if></xsl:if><xsl:if test="procedure/@visible='false'"><input type="hidden" name="p_procedure_fk" value="{procedure}" /><input type="hidden" name="p_procedure_fk_desc" value="{procedure_desc}" /></xsl:if><td align="center" width="10"><input type="hidden" name="p_valdation_id" value="{valdation_id}" /><xsl:if test="not(@nodelete)"><img alt="apagar" src="{$path}/img/icon/ctx-delete.png" class="IGRP_delRow" rel="valdation"/></xsl:if></td><td align="center" width="10"><xsl:if test="not(@noupdate)"><img alt="editar" src="{$path}/img/icon/edit.png" class="IGRP_editRow" rel="valdation"/></xsl:if></td></tr></xsl:for-each></tbody></table><!--LEGENDA FINAL DA TABELA --><xsl:apply-templates mode="legend_color" select="rows/content/form/table/valdation/legend_color"/