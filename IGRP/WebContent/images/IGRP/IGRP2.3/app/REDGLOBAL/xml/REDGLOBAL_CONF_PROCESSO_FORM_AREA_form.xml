<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet href="../REDGLOBAL_CONF_PROCESSO_FORM_AREA_form.xsl" type="text/xsl"?>
<rows>
    <site>
        <welcome_note>Ola</welcome_note>
        <footer_note>2011 - Copyright NOSI</footer_note>
        <user_name>red-igrp</user_name>
        <button type="specific">
            <title>Sair</title>
            <app>RED</app>
            <page />
            <link>#</link>
            <target>_self</target>
            <img>exit.png</img>
        </button>
    </site>
    <page>CONF_PROCESSO</page>
    <app>REDGLOBAL</app>
    <plsql>
        <action>FORM_AREA</action>
        <package_instance></package_instance>
        <package_db>redwf.GLB_AREA_2_DB</package_db>
        <package_html>redwf.GLB_AREA_2_HTML</package_html>
        <table_name>redwf.glb_area</table_name>
        <with_replace>true</with_replace>
        <with_label>true</with_label>
        <with_biztalk>false</with_biztalk>
        <dynamic_menu>false</dynamic_menu>
        <copy_menu>false</copy_menu>
        <package_copy_db />
        <package_copy_html />
        <preserve_url>https://nosiappsdev.gov.cv/redglobal_dev/red.form_designer_db.preserve_your_code</preserve_url>
    </plsql>
    <content>
        <title>Registar/Editar Area</title>
        <page_title type="sectionheader" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <page_title_text type="text" name="p_page_title_text" persist="true" maxlength="4000">
                    <value>
                        <![CDATA[Registar/Editar Area]]>
                    </value>
                </page_title_text>
            </fields>
        </page_title>
        <fmn type="toolsbar" xml-type="toolsbar" gen-type="menu" gen-group="">
            <item type="specific" code="" rel="gravar">
                <title>Gravar</title>
                <app>REDGLOBAL</app>
                <page>CONF_PROCESSO</page>
                <link>FORM_AREA</link>
                <target>submit</target>
                <img>success|fa-save</img>
                <params>success||fa-save|www</params>
            </item>
            <item type="specific" code="" rel="fechar">
                <title>Fechar</title>
                <app>REDGLOBAL</app>
                <page>CONF_PROCESSO</page>
                <link>CON_AREA</link>
                <target>_close</target>
                <img>danger|fa-window-close</img>
                <params>danger||fa-window-close|www</params>
            </item>
        </fmn>
        <form_1 type="form" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <env_fk name="p_env_fk" type="select" required="true" change="true" maxlength="22" multiple="false" disabled="false" right="true">
                    <label>Aplicacao</label>
                    <list>
                        <option>
                            <text>Option 1</text>
                            <value>1</value>
                        </option>
                        <option>
                            <text>Option 2</text>
                            <value>2</value>
                        </option>
                        <option>
                            <text>Option 3</text>
                            <value>3</value>
                        </option>
                        <option>
                            <text>Option 4</text>
                            <value>4</value>
                        </option>
                    </list>
                </env_fk>
                <sep_area type="separator" maxlength="30" placeholder="" right="false">
                    <label>Area Info</label>
                </sep_area>
                <code name="p_code" type="text" required="true" maxlength="20" readonly="true" change="false" disabled="false" placeholder="" right="false">
                    <label>Codigo</label>
                    <value>Mollit sed rem voluptatem sit</value>
                </code>
                <name name="p_name" type="text" required="true" maxlength="100" change="false" readonly="false" disabled="false" placeholder="" right="true">
                    <label>Nome</label>
                    <value>Anim unde elit sit rem</value>
                </name>
                <description name="p_description" type="textarea" maxlength="4000" required="false" change="false" readonly="false" disabled="false" placeholder="" right="false">
                    <label>Descricao</label>
                    <value>Omnis sit natus lorem anim natus dolor ipsum officia aperiam accusantium ut labore aliqua voluptatem</value>
                </description>
                <status name="p_status" type="checkbox" maxlength="1" required="false" change="false" readonly="false" disabled="false" right="false" check="true">
                    <label>Activo</label>
                    <value>1</value>
                </status>
                <hidden name="p_id" type="hidden" maxlength="30" tag="id"/>
                <hidden name="p_self_fk" type="hidden" maxlength="30" tag="self_fk"/>
            </fields>
        </form_1>
    </content>
</rows>