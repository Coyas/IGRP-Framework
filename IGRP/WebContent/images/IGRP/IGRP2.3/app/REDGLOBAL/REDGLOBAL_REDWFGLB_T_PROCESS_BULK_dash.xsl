		  <!--START PAGE--><form action="#" method="post" id="formular_default" name="formular_default" class="default_filter"><!--FILTRO --><!--START FORM--><div class="box-content"><!--HIDDEN FIELDS--><xsl:apply-templates mode="form-hidden" select="rows/content/filter/value" /><xsl:if test="rows/content/filter/label/proc_tp_fk"><label><span><xsl:value-of select="rows/content/filter/label/proc_tp_fk"/></span><select name="{rows/content/filter/list/proc_tp_fk/@name}" class=""><xsl:for-each select="rows/content/filter/list/proc_tp_fk/option"><option value="{value}"><xsl:if test="@selected='true'"><xsl:attribute name="selected">selected</xsl:attribute></xsl:if><xsl:value-of select="text"/></option></xsl:for-each></select></label></xsl:if><xsl:if test="rows/content/filter/label/acti_call_fk"><label><span><xsl:value-of select="rows/content/filter/label/acti_call_fk"/></span><input type="number" name="{rows/content/filter/value/acti_call_fk/@name}" value="{rows/content/filter/value/acti_call_fk}"  class="number" maxlength="22" /></label></xsl:if><xsl:if test="rows/content/filter/label/user_fk"><label><span><xsl:value-of select="rows/content/filter/label/user_fk"/></span><input type="number" name="{rows/content/filter/value/user_fk/@name}" value="{rows/content/filter/value/user_fk}"  class="number" maxlength="22" /></label></xsl:if><xsl:if test="rows/content/filter/label/date_created"><label><span><xsl:value-of select="rows/content/filter/label/date_created"/></span><input type="date" name="{rows/content/filter/value/date_created/@name}" value="{rows/content/filter/value/date_created}" class="IGRP_datepicker date" /></label></xsl:if><xsl:if test="rows/content/filter/label/date_started"><label><span><xsl:value-of select="rows/content/filter/label/date_started"/></span><input type="date" name="{rows/content/filter/value/date_started/@name}" value="{rows/content/filter/value/date_started}" class="IGRP_datepicker date" /></label></xsl:if><xsl:if test="rows/content/filter/label/date_ended"><label><span><xsl:value-of select="rows/content/filter/label/date_ended"/></span><input type="date" name="{rows/content/filter/value/date_ended/@name}" value="{rows/content/filter/value/date_ended}" class="IGRP_datepicker date" /></label></xsl:if><xsl:if test="rows/content/filter/label/status"><label><span><xsl:value-of select="rows/content/filter/label/status"/></span><input type="checkbox" name="{rows/content/filter/value/status/@name}" value="1"  class=""><xsl:if test="rows/content/filter/value/status='1'"><xsl:attribute name="checked">checked</xsl:attribute></xsl:if></input></label></xsl:if><xsl:if test="rows/content/filter/label/remarks"><label><span><xsl:value-of select="rows/content/filter/label/remarks"/></span><input type="text" name="{rows/content/filter/value/remarks/@name}" value="{rows/content/filter/value/remarks}"  class="" maxlength="4000" /></label></xsl:if><xsl:if test="rows/content/filter/label/num"><label><span><xsl:value-of select="rows/content/filter/label/num"/></span><input type="number" name="{rows/content/filter/value/num/@name}" value="{rows/content/filter/value/num}"  class="number" maxlength="22" /></label></xsl:if><xsl:if test="rows/content/filter/label/search"><label><span><xsl:value-of select="rows/content/filter/label/search"/></span><input type="text" name="{rows/content/filter/value/search/@name}" value="{rows/content/filter/value/search}"  class="" maxlength="1000" /></label></xsl:if><!--TOOLBAR FILTER--><xsl:apply-templates mode="button-bar" select="rows/content/filter/tools-bar" /></div><!--END FORM--><!--TOOL BAR--><xsl:apply-templates mode="tools-bar" select="rows/content/table/tools-bar" /><!-- CONTEXT-MENU --><xsl:apply-templates mode="context-menu" select="rows/content/table/context-menu"/><!--INICIO DA TABELA--><div class="table"><table class="sortable IGRP_table"><!--CABECALHO DA TABELA--><thead><tr><xsl:if test="rows/content/table/label/id"><th align="center"><xsl:value-of select="rows/content/table/label/id"/></th></xsl:if><xsl:if test="rows/content/table/label/proc_tp_fk"><th align="center"><xsl:value-of select="rows/content/table/label/proc_tp_fk"/></th></xsl:if><xsl:if test="rows/content/table/label/acti_call_fk"><th align="center"><xsl:value-of select="rows/content/table/label/acti_call_fk"/></th></xsl:if><xsl:if test="rows/content/table/label/user_fk"><th align="center"><xsl:value-of select="rows/content/table/label/user_fk"/></th></xsl:if><xsl:if test="rows/content/table/label/date_created"><th align="center"><xsl:value-of select="rows/content/table/label/date_created"/></th></xsl:if><xsl:if test="rows/content/table/label/date_started"><th align="center"><xsl:value-of select="rows/content/table/label/date_started"/></th></xsl:if><xsl:if test="rows/content/table/label/date_ended"><th align="center"><xsl:value-of select="rows/content/table/label/date_ended"/></th></xsl:if><xsl:if test="rows/content/table/label/status"><th class="checkbox sorttable_nosort" align="center"><input type="checkbox" name="p_status_all" value="{value}" class="IGRP_checkall" /></th></xsl:if><xsl:if test="rows/content/table/label/remarks"><th align="left"><xsl:value-of select="rows/content/table/label/remarks"/></th></xsl:if><xsl:if test="rows/content/table/label/num"><th align="center"><xsl:value-of select="rows/content/table/label/num"/></th></xsl:if><xsl:if test="rows/content/table/label/search"><th align="left"><xsl:value-of select="rows/content/table/label/search"/></th></xsl:if></tr></thead><!--CORPO DA TABELA--><tbody><xsl:for-each select="rows/content/table/value/row"><xsl:variable name="pos" select="(position()+1) mod 2"/><tr class="IGRP_contextMenu"><xsl:apply-templates mode="context-param" select="context-menu" /><xsl:if test="id"><td class="table_row{$pos}" align="center"><xsl:value-of select="id" /></td></xsl:if><xsl:if test="proc_tp_fk"><td class="table_row{$pos}" align="center"><xsl:value-of select="proc_tp_fk" /></td></xsl:if><xsl:if test="acti_call_fk"><td class="table_row{$pos}" align="center"><xsl:value-of select="acti_call_fk" /></td></xsl:if><xsl:if test="user_fk"><td class="table_row{$pos}" align="center"><xsl:value-of select="user_fk" /></td></xsl:if><xsl:if test="date_created"><td class="table_row{$pos}" align="center"><xsl:value-of select="date_created" /></td></xsl:if><xsl:if test="date_started"><td class="table_row{$pos}" align="center"><xsl:value-of select="date_started" /></td></xsl:if><xsl:if test="date_ended"><td class="table_row{$pos}" align="center"><xsl:value-of select="date_ended" /></td></xsl:if><xsl:if test="status"><td class="table_row{$pos} checkbox" align="left"><input type="checkbox" name="p_status" value="{status}"><xsl:if test="status_check=status"><xsl:attribute name="checked">checked</xsl:attribute></xsl:if></input><xsl:value-of select="status_desc" /></td></xsl:if><xsl:if test="remarks"><td class="table_row{$pos}" align="left"><xsl:value-of select="remarks" /></td></xsl:if><xsl:if test="num"><td class="table_row{$pos}" align="center"><xsl:value-of select="num" /></td></xsl:if><xsl:if test="search"><td class="table_row{$pos}" align="left"><xsl:value-of select="search" /></td></xsl:if></tr></xsl:for-each></tbody></table></div><!--FIM DA TABELA--></form><!--END PAGE-->


