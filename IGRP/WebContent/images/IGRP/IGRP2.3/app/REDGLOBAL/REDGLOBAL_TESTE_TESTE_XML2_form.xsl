s" select="'subtitle'" /></xsl:call-template></xsl:if><xsl:if test="rows/content/form/label/separadorlist"><xsl:call-template name="tools-bar-form-group"><xsl:with-param name="rel" select="'separadorlist'" /></xsl:call-template></xsl:if><xsl:if test="rows/content/form/label/sp_nome"><label><span><xsl:value-of select="rows/content/form/label/sp_nome"/></span><input type="text" name="{rows/content/form/value/sp_nome/@name}" value="{rows/content/form/value/sp_nome}"  rel="F_separadorlist"   maxlength="30"  class=""><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/sp_nome" /></xsl:call-template></input></label></xsl:if><xsl:if test="rows/content/form/label/sp_bi"><label><span><xsl:value-of select="rows/content/form/label/sp_bi"/></span><input type="text" name="{rows/content/form/value/sp_bi/@name}" value="{rows/content/form/value/sp_bi}"  rel="F_separadorlist"   maxlength="30"  class=""><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/sp_bi" /></xsl:call-template></input><xsl:call-template name="lookup-tool"><xsl:with-param name="page" select="'LOOKUP'" /><xsl:with-param name="action" select="'LOOKUP_BI'" /><xsl:with-param name="name" select="rows/content/form/value/sp_bi/@name" /><xsl:with-param name="js_lookup" select="rows/content/form/lookup/sp_bi" /></xsl:call-template></label></xsl:if><xsl:if test="rows/content/form/label/sp_data"><label><span><xsl:value-of select="rows/content/form/label/sp_data"/></span><input type="text" name="{rows/content/form/value/sp_data/@name}" value="{rows/content/form/value/sp_data}"  rel="F_separadorlist"  class="IGRP_datepicker date"><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/sp_data" /></xsl:call-template></input></label></xsl:if><xsl:if test="rows/content/form/label/sp_sexo"><label><a id="idp_sp_sexo" /><span><xsl:value-of select="rows/content/form/label/sp_sexo"/></span><select name="{rows/content/form/list/sp_sexo/@name}" rel="F_separadorlist"  class="IGRP_change"><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/sp_sexo" /></xsl:call-template><xsl:for-each select="rows/content/form/list/sp_sexo/option"><option value="{value}"><xsl:if test="@selected='true'"><xsl:attribute name="selected">selected</xsl:attribute></xsl:if><xsl:value-of select="text"/></option></xsl:for-each></select></label></xsl:if><xsl:if test="rows/content/form/label/separadorlist"><!--INICIO DA TABELA--><div class="simple-table"><table rel="T_separadorlist"><!--CABECALHO DA TABELA--><thead><tr><xsl:if test="not(rows/content/form/table/separadorlist/label/sp_nome/@visible)"><th><xsl:value-of select="rows/content/form/table/separadorlist/label/sp_nome"/></th></xsl:if><xsl:if test="not(rows/content/form/table/separadorlist/label/sp_bi/@visible)"><th><xsl:value-of select="rows/content/form/table/separadorlist/label/sp_bi"/></th></xsl:if><xsl:if test="not(rows/content/form/table/separadorlist/label/sp_data/@visible)"><th><xsl:value-of select="rows/content/form/table/separadorlist/label/sp_data"/></th></xsl:if><xsl:if test="not(rows/content/form/table/separadorlist/label/sp_sexo/@visible)"><th><xsl:value-of select="rows/content/form/table/separadorlist/label/sp_sexo"/></th></xsl:if><th align="center" width="10"></th><th align="center" width="10"></th></tr></thead><!--CORPO DA TABELA--><tbody><xsl:for-each select="rows/content/form/table/separadorlist/value/row"><tr><xsl:if test="not(sp_nome/@visible)"><td><xsl:value-of select="sp_nome_desc" /><input type="hidden" name="p_separadorlist_id" value="{separadorlist_id}" /><input type="hidden" name="p_sp_nome_fk" value="{sp_nome}" /><input type="hidden" name="p_sp_nome_fk_desc" value="{sp_nome_desc}" /></td></xsl:if><xsl:if test="sp_nome/@visible='false'"><input type="hidden" name="p_sp_nome_fk" value="{sp_nome}" /><input type="hidden" name="p_sp_nome_fk_desc" value="{sp_nome_desc}" /></xsl:if><xsl:if test="not(sp_bi/@visible)"><td><xsl:value-of select="sp_bi_desc" /><input type="hidden" name="p_sp_bi_fk" value="{sp_bi}" /><input type="hidden" name="p_sp_bi_fk_desc" value="{sp_bi_desc}" /></td></xsl:if><xsl:if test="sp_bi/@visible='false'"><input type="hidden" name="p_sp_bi_fk" value="{sp_bi}" /><input type="hidden" name="p_sp_bi_fk_desc" value="{sp_bi_desc}" /></xsl:if><xsl:if test="not(sp_data/@visible)"><td><xsl:value-of select="sp_data_desc" /><input type="hidden" name="p_sp_data_fk" value="{sp_data}" /><input type="hidden" name="p_sp_data_fk_desc" value="{sp_data_desc}" /></td></xsl:if><xsl:if test="sp_data/@visible='false'"><input type="hidden" name="p_sp_data_fk" value="{sp_data}" /><input type="hidden" name="p_sp_data_fk_desc" value="{sp_data_desc}" /></xsl:if><xsl:if test="not(sp_sexo/@visible)"><td><xsl:value-of select="sp_sexo_desc" /><input type="hidden" name="p_sp_sexo_fk" value="{sp_sexo}" /><input type="hidden" name="p_sp_sexo_fk_desc" value="{sp_sexo_desc}" /></td></xsl:if><xsl:if test="sp_sexo/@visible='false'"><input type="hidden" name="p_sp_sexo_fk" value="{sp_sexo}" /><input type="hidden" name="p_sp_sexo_fk_desc" value="{sp_sexo_desc}" /></xsl:if><td align="center" width="10"><img alt="apagar" src="{$path}/img/icon/ctx-delete.png" class="IGRP_delRow" rel="separadorlist"/></td><td align="center" width="10"><img alt="editar" src="{$path}/img/icon/edit.png" class="IGRP_editRow" rel="separadorlist"/></td></tr></xsl:for-each></tbody></table></div><!--FIM DA TABELA--></xsl:if></form></div><!--END FORM-->


