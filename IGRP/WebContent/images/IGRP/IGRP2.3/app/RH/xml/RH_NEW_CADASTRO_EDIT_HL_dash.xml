        <package_copy_html />
        <preserve_url>https://nosiappsdev.gov.cv/redglobal_dev/red.form_designer_db.preserve_your_code</preserve_url>
    </plsql>
    <content>
        <title>Editar Habilitação Literária</title>
        <box_1 type="box" xml-type="form" gen-type="container" gen-group="">
            <fields/>
        </box_1>
        <form_1 type="form" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <pais name="p_pais" type="select" multiple="false" maxlength="100" required="false" change="false" disabled="false" right="false">
                    <label>País</label>
                    <list>
                        <option>
                            <text>Option 1</text>
                            <value>1</value>
                        </option>
                        <option>
                            <text>Option 2</text>
                            <value>2</value>
                        </option>
                        <option>
                            <text>Option 3</text>
                            <value>3</value>
                        </option>
                        <option>
                            <text>Option 4</text>
                            <value>4</value>
                        </option>
                    </list>
                </pais>
                <estabelecimento name="p_estabelecimento" type="select" multiple="false" maxlength="100" required="false" change="false" disabled="false" right="false">
                    <label>Estabelecimento</label>
                    <list>
                        <option>
                            <text>Option 1</text>
                            <value>1</value>
                        </option>
                        <option>
                            <text>Option 2</text>
                            <value>2</value>
                        </option>
                        <option>
                            <text>Option 3</text>
                            <value>3</value>
                        </option>
                        <option>
                            <text>Option 4</text>
                            <value>4</value>
                        </option>
                    </list>
                </estabelecimento>
                <nivel_de_habilitacao name="p_nivel_de_habilitacao" type="select" multiple="false" maxlength="30" required="false" change="false" disabled="false" right="false">
                    <label>Nível de Habilitação</label>
                    <list>
                        <option>
                            <text>Option 1</text>
                            <value>1</value>
                        </option>
                        <option>
                            <text>Option 2</text>
                            <value>2</value>
                        </option>
                        <option>
                            <text>Option 3</text>
                            <value>3</value>
                        </option>
                        <option>
                            <text>Option 4</text>
                            <value>4</value>
                        </option>
                    </list>
                </nivel_de_habilitacao>
                <habilitacao_literaria name="p_habilitacao_literaria" type="lookup" action="NEW_HL" page="NEW_CADASTRO" app="RH" lookup_type="LOOKUP" class="default" maxlength="100" required="false" change="false" readonly="false" disabled="false" placeholder="" right="false">
                    <label>Habilitação Literária</label>
                    <value/>
                    <lookup>http://igrp.teste.gov.cv/images/IGRP.bootstrap/app/BOOTSTRAP/xml/lookup.test.xml</lookup>
                </habilitacao_literaria>
                <data_inicio name="p_data_inicio" type="date" format="IGRP_datePicker" maxlength="30" required="false" change="false" readonly="false" disabled="false" placeholder="" right="false" class="default">
                    <label>Data Inicio</label>
                    <value/>
                </data_inicio>
                <data_fim name="p_data_fim" type="date" format="IGRP_datePicker" maxlength="30" required="false" change="false" readonly="false" disabled="false" placeholder="" right="false" class="default">
                    <label>Data Fim</label>
                    <value/>
                </data_fim>
                <concluido name="p_concluido" type="radiolist" maxlength="30" required="false" change="false" readonly="false" disabled="false" right="false">
                    <label>Concluído</label>
                    <list>
                        <option>
                            <text>Option 1</text>
                            <value>1</value>
                        </option>
                        <option>
                            <text>Option 2</text>
                            <value>2</value>
                        </option>
                        <option>
                            <text>Option 3</text>
                            <value>3</value>
                        </option>
                        <option>
                            <text>Option 4</text>
                            <value>4</value>
                        </option>
                    </list>
                </concluido>
                <observacoes name="p_observacoes" type="textarea" maxlength="30" required="false" change="false" readonly="false" disabled="false" placeholder="" right="false">
                    <label>Observações</label>
                    <value>Adipiscing amet sit unde sit aliqua amet totam sit laudantium aperiam elit stract anim sed</value>
                </observacoes>
            </fields>
        </form_1>
        <separatorlist_1 type="separatorlist" xml-type="separatorlist" title="Anexos" gen-type="container" gen-group="true">
            <fields>
                <documento name="p_documento" type="file" accept="file_extension" multiple="false" maxlength="4000" required="false" disabled="false" right="false" visible="true" class="default" desc="true">
                    <label>Documento</label>
                    <value/>
                </documento>
            </fields>
            <table>
                <value>
                    <row>
                        <documento name="p_documento" desc="true"/>
                        <documento_desc name="p_documento_desc"/>
                    </row>
                    <row>
                        <documento name="p_documento" desc="true"/>
                        <documento_desc name="p_documento_desc"/>
                    </row>
                    <row>
                        <documento name="p_documento" desc="true"/>
                        <documento_desc name="p_documento_desc"/>
                    </row>
                    <row>
                        <documento name="p_documento" desc="true"/>
                        <documento_desc name="p_documento_desc"/>
                    </row>
                </value>
            </table>
        </separatorlist_1>
        <sectionheader_1 type="sectionheader" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <sectionheader_1_text type="text" name="p_sectionheader_1_text" persist="true" maxlength="4000">
                    <value>
                        <![CDATA[Habilitações Literárias]]>
                    </value>
                </sectionheader_1_text>
            </fields>
        </sectionheader_1>
        <toolsbar_1 type="toolsbar" xml-type="toolsbar" gen-type="menu" gen-group="">
            <item type="specific" code="" rel="gravar">
                <title>Gravar</title>
                <app>RH</app>
                <page>NEW_CADASTRO</page>
                <link>EDIT_HL</link>
                <target>alert_submit</target>
                <img>success|fa-floppy-o</img>
                <parameter>http://igrp.teste.gov.cv/images/IGRP/IGRP2.3/app/RH/xml/RH_NEW_CADASTRO_EDIT_HL_dash.xml</parameter>
                <params>success||fa-floppy-o|www</params>
            </item>
        </toolsbar_1>
    </content>
</rows>