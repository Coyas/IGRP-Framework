    <content>
        <title>Lista Anexos Familiares e Dependetes</title>
        <box_1 type="box" xml-type="form" gen-type="container" gen-group="">
            <fields/>
        </box_1>
        <sectionheader_1 type="sectionheader" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <sectionheader_1_text type="text" name="p_sectionheader_1_text" persist="true" maxlength="4000">
                    <value>
                        <![CDATA[Anexos]]>
                    </value>
                </sectionheader_1_text>
            </fields>
        </sectionheader_1>
        <box_2 type="box" xml-type="form" gen-type="container" gen-group="">
            <fields/>
        </box_2>
        <form_1 type="form" xml-type="form" title="Funcionário" gen-type="container" gen-group="">
            <fields>
                <nome name="p_nome" type="text" maxlength="100" required="false" change="false" readonly="true" disabled="false" placeholder="" right="false">
                    <label>Nome</label>
                    <value>Officia accusantium aperiam doloremque aperiam</value>
                </nome>
            </fields>
        </form_1>
        <toolsbar_1 type="toolsbar" xml-type="toolsbar" gen-type="menu" gen-group="">
            <item type="specific" code="" rel="novo">
                <title>Novo</title>
                <app>RH</app>
                <page>NEW_CADASTRO</page>
                <link>ANEXO_FAMILIAR</link>
                <target>_blank</target>
                <img>success|fa-plus-square-o</img>
                <parameter>http://igrp.teste.gov.cv/images/IGRP/IGRP2.3/app/RH/xml/RH_NEW_CADASTRO_ANEXO_FAMILIAR_dash.xml</parameter>
                <params>success||fa-plus-square-o|www</params>
            </item>
            <item type="specific" code="" rel="actualizar">
                <title>Actualizar</title>
                <app>RH</app>
                <page>NEW_CADASTRO</page>
                <link>LST_ANEXO_FAM</link>
                <target>_self</target>
                <img>info|fa-random</img>
                <parameter>http://igrp.teste.gov.cv/images/IGRP/IGRP2.3/app/RH/xml/RH_NEW_CADASTRO_LST_ANEXO_FAM_dash.xml</parameter>
                <params>info||fa-random|www</params>
            </item>
        </toolsbar_1>
        <form_2 type="form" xml-type="form" title="Agregado Familiar" gen-type="container" gen-group="">
            <fields>
                <nome_familiar name="p_nome_familiar" type="text" maxlength="100" required="false" change="false" readonly="true" disabled="false" placeholder="" right="false">
                    <label>Nome</label>
                    <value>Lorem sit labore consectetur aliqua</value>
                </nome_familiar>
            </fields>
        </form_2>
        <table_1 type="table" xml-type="table" gen-type="container" gen-group="true">
            <fields>
                <anexos name="p_anexos" type="text" maxlength="100" align="left" lookup_parser="false">
                    <label>Anexos</label>
                </anexos>
                <data_de_insercao name="p_data_de_insercao" type="date" format="IGRP_datePicker" maxlength="30" align="left" lookup_parser="false">
                    <label>Data de inserção</label>
                </data_de_insercao>
                <ver name="p_ver" type="link" target="_blank" class="link" iconColor="#333" iconClass="" img="fa-link" maxlength="4000" align="left" lookup_parser="false" desc="true">
                    <label>Ver</label>
                </ver>
            </fields>
            <table>
                <value>
                    <row>
                        <context-menu>
                            <param>p1=linha1</param>
                            <param>p2=linha1</param>
                        </context-menu>
                        <anexos name="p_anexos">Consectetur aliqua ut sit iste</anexos>
                        <data_de_insercao name="p_data_de_insercao"/>
                        <ver name="p_ver" desc="true">http://wisar.edu/pivasvov</ver>
                        <ver_desc name="p_ver_desc">http://wisar.edu/pivasvov</ver_desc>
                    </row>
                    <row>
                        <context-menu>
                            <param>p1=linha1</param>
                            <param>p2=linha1</param>
                        </context-menu>
                        <anexos name="p_anexos">Natus sit rem ut laudantium</anexos>
