        <tabcontent_1 type="tabcontent" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <dados_gerais name="p_dados_gerais" type="button" outline="false" iconColor="#333" iconClass="" img="fa-cog" maxlength="50">
