                <page>NEW_CADASTRO</page>
                <link>EDIT_FORM</link>
                <target>alert_submit</target>
                <img>success|fa-floppy-o</img>
                <parameter>http://igrp.teste.gov.cv/images/IGRP/IGRP2.3/app/RH/xml/RH_NEW_CADASTRO_EDIT_FORM_dash.xml</parameter>
                <params>success||fa-floppy-o|www</params>
            </item>
        </toolsbar_1>
