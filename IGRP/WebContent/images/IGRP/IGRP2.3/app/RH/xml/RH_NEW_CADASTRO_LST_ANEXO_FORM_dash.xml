<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet href="../RH_NEW_CADASTRO_LST_ANEXO_FORM_dash.xsl" type="text/xsl"?>
<rows>
    <site>
        <welcome_note>Ola</welcome_note>
        <footer_note>2011 - Copyright NOSI</footer_note>
        <user_name>red-igrp</user_name>
        <button type="specific">
            <title>Sair</title>
            <app>RED</app>
            <page />
            <link>#</link>
            <target>_self</target>
            <img>exit.png</img>
        </button>
    </site>
    <page>NEW_CADASTRO</page>
    <app>RH</app>
    <plsql>
        <action>LST_ANEXO_FORM</action>
        <package_instance></package_instance>
        <package_db></package_db>
        <package_html></package_html>
        <table_name></table_name>
        <with_replace>false</with_replace>
        <with_label>false</with_label>
        <with_biztalk>false</with_biztalk>
        <dynamic_menu>false</dynamic_menu>
        <copy_menu>false</copy_menu>
        <package_copy_db />
        <package_copy_html />
        <preserve_url>https://nosiappsdev.gov.cv/redglobal_dev/red.form_designer_db.preserve_your_code</preserve_url>
    </plsql>
    <content>
        <title>Lista Anexos Formação</title>
        <box_1 type="box" xml-type="form" gen-type="container" gen-group="">
            <fields/>
        </box_1>
        <sectionheader_1 type="sectionheader" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <sectionheader_1_text type="text" name="p_sectionheader_1_text" persist="true" maxlength="4000">
                    <value>
                        <![CDATA[Anexos]]>
                    </value>
                </sectionheader_1_text>
            </fields>
        </sectionheader_1>
        <table_1 type="table" xml-type="table" gen-type="container" gen-group="true">
            <fields>
                <anexo name="p_anexo" type="text" maxlength="50" align="left" lookup_parser="false">
                    <label>Anexo</label>
                </anexo>
                <data_de_insercao name="p_data_de_insercao" type="date" format="IGRP_datePicker" maxlength="30" align="left" lookup_parser="false">
                    <label>Data de Inserção</label>
                </data_de_insercao>
                <ver name="p_ver" type="link" target="_self" class="link" iconColor="#333" iconClass="" img="fa-link" maxlength="4000" align="left" lookup_parser="false" desc="true">
                    <label>Ver</label>
                </ver>
            </fields>
            <table>
                <value>
                    <row>
                        <context-menu>
                            <param>p1=linha1</param>
                            <param>p2=linha1</param>
                        </context-menu>
                        <anexo name="p_anexo">Unde natus labore rem sit</anexo>
                        <data_de_insercao name="p_data_de_insercao"/>
                        <ver name="p_ver" desc="true">http://nol.edu/iwga</ver>
                        <ver_desc name="p_ver_desc">http://nol.edu/iwga</ver_desc>
                    </row>
                    <row>
                        <context-menu>
                            <param>p1=linha1</param>
                            <param>p2=linha1</param>
                        </context-menu>
                        <anexo name="p_anexo">Laudantium ipsum rem magna rem</anexo>
                        <data_de_insercao name="p_data_de_insercao"/>
                        <ver name="p_ver" desc="true">http://ve.co.uk/rowozel</ver>
                        <ver_desc name="p_ver_desc">http://ve.co.uk/rowozel</ver_desc>
                    </row>
                    <row>
                        <context-menu>
                            <param>p1=linha1</param>
                            <param>p2=linha1</param>
                        </context-menu>
                        <anexo name="p_anexo">Ipsum unde iste totam laudantium</anexo>
                        <data_de_insercao name="p_data_de_insercao"/>
                        <ver name="p_ver" desc="true">http://rifeti.co.uk/ak</ver>
                        <ver_desc name="p_ver_desc">http://rifeti.co.uk/ak</ver_desc>
                    </row>
                    <row>
                        <context-menu>
                            <param>p1=linha1</param>
                            <param>p2=linha1</param>
                        </context-menu>
                        <anexo name="p_anexo">Perspiciatis rem doloremque accusantium dolor</anexo>
                        <data_de_insercao name="p_data_de_insercao"/>
                        <ver name="p_ver" desc="true">http://voigu.io/baor</ver>
                        <ver_desc name="p_ver_desc">http://voigu.io/baor</ver_desc>
                    </row>
                </value>
                <context-menu>
                    <item type="specific" code="" class="danger" rel="eliminar">
                        <title>Eliminar</title>
                        <app>RH</app>
                        <page>NEW_CADASTRO</page>
                        <link>LST_ANEXO_IDIOMA</link>
                        <parameter>http://igrp.teste.gov.cv/images/IGRP/IGRP2.3/app/RH/xml/RH_NEW_CADASTRO_LST_ANEXO_IDIOMA_dash.xml</parameter>
                        <target>alert_submit</target>
                        <img>danger|fa-close</img>
                    </item>
                </context-menu>
            </table>
        </table_1>
        <toolsbar_1 type="toolsbar" xml-type="toolsbar" gen-type="menu" gen-group="">
            <item type="specific" code="" rel="novo">
                <title>Novo</title>
                <app>RH</app>
                <page>NEW_CADASTRO</page>
                <link>ANEXO_FORM</link>
                <target>_blank</target>
                <img>success|fa-plus-square-o</img>
                <parameter>http://igrp.teste.gov.cv/images/IGRP/IGRP2.3/app/RH/xml/RH_NEW_CADASTRO_ANEXO_FORM_dash.xml</parameter>
                <params>success||fa-plus-square-o|www</params>
            </item>
            <item type="specific" code="" rel="actualizar">
                <title>Actualizar</title>
                <app>RH</app>
                <page>NEW_CADASTRO</page>
                <link>LST_ANEXO_FORM</link>
                <target>_self</target>
                <img>info|fa-random</img>
                <parameter>http://igrp.teste.gov.cv/images/IGRP/IGRP2.3/app/RH/xml/RH_NEW_CADASTRO_LST_ANEXO_FORM_dash.xml</parameter>
                <params>info||fa-random|www</params>
            </item>
        </toolsbar_1>
        <form_1 type="form" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <nome name="p_nome" type="text" maxlength="100" required="false" change="false" readonly="true" disabled="false" placeholder="" right="false">
                    <label>Nome</label>
                    <value>Laudantium magna amet magna sed</value>
                </nome>
            </fields>
        </form_1>
    </content>
</rows>