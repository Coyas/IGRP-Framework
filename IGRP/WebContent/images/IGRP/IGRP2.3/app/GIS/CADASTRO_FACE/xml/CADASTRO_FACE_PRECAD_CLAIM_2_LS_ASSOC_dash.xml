    <content>
        <title>Consultar Associação</title>
        <page_title type="sectionheader" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <page_title_text type="text" name="p_page_title_text" persist="true" maxlength="100">
                    <value>
                        <![CDATA[Consultar Associação]]>
                    </value>
                </page_title_text>
            </fields>
        </page_title>
        <form_1 type="form" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <cidadevila_ou_bairro name="p_cidadevila_ou_bairro" type="select" multiple="false" required="false" change="true" disabled="false" right="false">
                    <label>Cidade/Vila ou Bairro</label>
                    <list>
                        <option>
                            <text>Option 1</text>
                            <value>1</value>
                        </option>
                        <option>
                            <text>Option 2</text>
                            <value>2</value>
                        </option>
                        <option>
                            <text>Option 3</text>
                            <value>3</value>
                        </option>
                        <option>
                            <text>Option 4</text>
                            <value>4</value>
                        </option>
                    </list>
                </cidadevila_ou_bairro>
                <bairro_lugar name="p_bairro_lugar" type="select" multiple="false" required="false" change="false" disabled="false" right="false">
                    <label>Bairro ou Lugar</label>
                    <list>
                        <option>
                            <text>Option 1</text>
                            <value>1</value>
                        </option>
                        <option>
                            <text>Option 2</text>
                            <value>2</value>
                        </option>
                        <option>
                            <text>Option 3</text>
                            <value>3</value>
                        </option>
                        <option>
                            <text>Option 4</text>
                            <value>4</value>
                        </option>
                    </list>
                </bairro_lugar>
                <data_de name="p_data_de" type="date" format="IGRP_datePicker" required="false" change="false" readonly="false" disabled="false" maxlength="30" right="false" class="default">
                    <label>Data De</label>
                    <value/>
                </data_de>
                <data_ate name="p_data_ate" type="date" format="IGRP_datePicker" required="false" change="false" readonly="false" disabled="false" maxlength="30" right="false" class="default">
                    <label>Data Ate</label>
                    <value/>
                </data_ate>
                <matriz name="p_matriz" type="lookup" action="LS_ASSOC" page="PRECAD_CLAIM_2" app="CADASTRO_FACE" class="default" required="false" change="false" readonly="false" disabled="false" maxlength="30" right="false">
                    <label>Matriz</label>
                    <value/>
                    <lookup>http://igrp.teste.gov.cv/images/IGRP.bootstrap/app/BOOTSTRAP/xml/lookup.test.xml</lookup>
                </matriz>
                <fracao_matriz name="p_fracao_matriz" type="text" required="false" change="false" readonly="false" disabled="false" maxlength="30" right="false">
