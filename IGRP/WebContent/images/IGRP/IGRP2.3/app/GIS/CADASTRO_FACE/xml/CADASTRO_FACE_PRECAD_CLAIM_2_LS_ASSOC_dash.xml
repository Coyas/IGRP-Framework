                    <label>Predial</label>
                    <value/>
                    <lookup>http://igrp.teste.gov.cv/images/IGRP.bootstrap/app/BOOTSTRAP/xml/lookup.test.xml</lookup>
                </predial>
                <fracao_predial name="p_fracao_predial" type="text" required="false" change="false" readonly="false" disabled="false" maxlength="30" right="false">
