                <ato_notarial name="p_ato_notarial" type="lookup" action="LS_ASSOC" page="PRECAD_CLAIM_2" app="CADASTRO_FACE" class="default" required="false" change="false" readonly="false" disabled="false" maxlength="30" right="false">
                    <label>Ato Notarial</label>
                    <value/>
                    <lookup>http://igrp.teste.gov.cv/images/IGRP.bootstrap/app/BOOTSTRAP/xml/lookup.test.xml</lookup>
                </ato_notarial>
                <poligono name="p_poligono" type="lookup" action="LS_ASSOC" page="PRECAD_CLAIM_2" app="CADASTRO_FACE" class="default" required="false" change="false" readonly="false" disabled="false" maxlength="30" right="false">
                    <label>Poligono</label>
                    <value/>
                    <lookup>http://igrp.teste.gov.cv/images/IGRP.bootstrap/app/BOOTSTRAP/xml/lookup.test.xml</lookup>
                </poligono>
                <hidden name="p_matriz_id" type="hidden" tag="matriz_id"/>
                <hidden name="p_concelho_id" type="hidden" tag="concelho_id"/>
                <hidden name="p_predial_id" type="hidden" tag="predial_id"/>
                <hidden name="p_notario_id" type="hidden" tag="notario_id"/>
            </fields>
            <tools-bar>
                <item type="form" code="" class="success" rel="pesquisar">
                    <title>Pesquisar</title>
                    <app>CADASTRO_FACE</app>
                    <page>PRECAD_CLAIM_2</page>
                    <link>LS_ASSOC</link>
                    <target>submit</target>
                    <img>success|fa-search</img>
                </item>
                <item type="form" code="" class="secondary" rel="novo">
                    <title>Novo</title>
                    <app>CADASTRO_FACE</app>
                    <page>PRECAD_CLAIM_2</page>
                    <link>LS_ASSOC</link>
                    <target>_blank</target>
                    <img>secondary|fa-plus-square</img>
                </item>
            </tools-bar>
