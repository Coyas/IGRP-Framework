<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet href="../CADASTRO_FACE_TASK_LS_TASK_dash.xsl" type="text/xsl"?>
<rows>
    <site>
        <welcome_note>Ola</welcome_note>
        <footer_note>2011 - Copyright NOSI</footer_note>
        <user_name>red-igrp</user_name>
        <button type="specific">
            <title>Sair</title>
            <app>RED</app>
            <page />
            <link>#</link>
            <target>_self</target>
            <img>exit.png</img>
        </button>
    </site>
    <page>TASK</page>
    <app>CADASTRO_FACE</app>
    <plsql>
        <action>LS_TASK</action>
        <package_instance>devnosi</package_instance>
        <package_db>PRECAD_LIST_TASK_DB</package_db>
        <package_html>PRECAD_LIST_TASK_HTML</package_html>
        <table_name></table_name>
        <with_replace>true</with_replace>
        <with_label>true</with_label>
        <with_biztalk>false</with_biztalk>
        <dynamic_menu>false</dynamic_menu>
        <copy_menu>false</copy_menu>
        <package_copy_db />
        <package_copy_html />
        <preserve_url>https://nosiappsdev.gov.cv/redglobal_dev/red.form_designer_db.preserve_your_code</preserve_url>
    </plsql>
    <content>
        <title>Lista Task Manager</title>
        <page_title type="sectionheader" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <page_title_text type="text" name="p_page_title_text" persist="true" maxlength="100">
                    <value>
                        <![CDATA[Lista de Tarefas]]>
                    </value>
                </page_title_text>
            </fields>
        </page_title>
        <form_1 type="form" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <criado_por name="p_criado_por" type="select" multiple="false" required="false" change="false" disabled="false" right="false">
                    <label>Criado Por</label>
                    <list>
                        <option>
                            <text>Option 1</text>
                            <value>1</value>
                        </option>
                        <option>
                            <text>Option 2</text>
                            <value>2</value>
                        </option>
                        <option>
                            <text>Option 3</text>
                            <value>3</value>
                        </option>
                        <option>
                            <text>Option 4</text>
                            <value>4</value>
                        </option>
                    </list>
                </criado_por>
                <atribuido_filter name="p_atribuido_filter" type="select" multiple="false" required="false" change="false" disabled="false" right="false">
                    <label>Atribuido Por</label>
                    <list>
                        <option>
                            <text>Option 1</text>
                            <value>1</value>
                        </option>
                        <option>
                            <text>Option 2</text>
                            <value>2</value>
                        </option>
                        <option>
                            <text>Option 3</text>
                            <value>3</value>
                        </option>
                        <option>
                            <text>Option 4</text>
                            <value>4</value>
                        </option>
                    </list>
                </atribuido_filter>
                <realizado_filter name="p_realizado_filter" type="select" multiple="false" required="false" change="false" disabled="false" right="false">
                    <label>Realizado Por</label>
                    <list>
                        <option>
                            <text>Option 1</text>
                            <value>1</value>
                        </option>
                        <option>
                            <text>Option 2</text>
                            <value>2</value>
                        </option>
                        <option>
                            <text>Option 3</text>
                            <value>3</value>
                        </option>
                        <option>
                            <text>Option 4</text>
                            <value>4</value>
                        </option>
                    </list>
                </realizado_filter>
                <por_fazer name="p_por_fazer" type="checkbox" required="false" change="false" readonly="false" disabled="false" right="false" check="true">
                    <label>Por fazer</label>
                    <value>1</value>
                </por_fazer>
                <realizado_check name="p_realizado_check" type="checkbox" required="false" change="false" readonly="false" disabled="false" right="false" check="true">
                    <label>Realizado</label>
                    <value>1</value>
                </realizado_check>
                <hidden name="p_p_response_back" type="hidden" tag="p_response_back"/>
            </fields>
            <tools-bar>
                <item type="form" code="" class="success" rel="novo">
                    <title>Novo</title>
                    <app>CADASTRO_FACE</app>
                    <page>TASK</page>
                    <link>NEW_TASK</link>
                    <target>modal</target>
                    <img>success|fa-plus-circle</img>
                </item>
                <item type="form" code="" class="primary" rel="pes">
                    <title>Pesquisar</title>
                    <app>CADASTRO_FACE</app>
                    <page>TASK</page>
                    <link>LS_TASK</link>
                    <target>submit</target>
                    <img>primary|fa-search</img>
                </item>
            </tools-bar>
        </form_1>
        <table_1 type="table" xml-type="table" gen-type="container" gen-group="true">
            <fields>
                <titulo name="p_titulo" type="text" align="left" lookup_parser="false">
                    <label>Titulo</label>
                </titulo>
                <atribuido_a name="p_atribuido_a" type="text" align="left" lookup_parser="false">
                    <label>Atribuido A</label>
                </atribuido_a>
                <realizado_por name="p_realizado_por" type="text" align="left" lookup_parser="false">
                    <label>Realizado Por</label>
                </realizado_por>
                <data_limite name="p_data_limite" type="date" format="IGRP_datePicker" align="left" lookup_parser="false">
                    <label>Data Limite</label>
                </data_limite>
                <data_realizacao name="p_data_realizacao" type="date" format="IGRP_datePicker" align="left" lookup_parser="false">
                    <label>Data Realização</label>
                </data_realizacao>
                <id name="p_ID" type="hidden" tag="id"/>
            </fields>
            <table>
                <value>
                    <row>
                        <context-menu>
                            <param>p1=linha1</param>
                            <param>p2=linha1</param>
                        </context-menu>
                        <titulo name="p_titulo">Lorem consectetur elit magna dolor</titulo>
                        <atribuido_a name="p_atribuido_a">Laudantium sit omnis ut sit</atribuido_a>
