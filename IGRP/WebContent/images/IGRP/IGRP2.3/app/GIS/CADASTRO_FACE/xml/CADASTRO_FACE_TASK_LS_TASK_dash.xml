                    </row>
                </value>
                <context-menu>
                    <item type="specific" code="" class="default" rel="editar">
                        <title>Editar</title>
                        <app>CADASTRO_FACE</app>
                        <page>TASK</page>
                        <link>LS_TASK</link>
                        <target>modal</target>
                        <img>default|fa-eyedropper</img>
                    </item>
                    <item type="specific" code="" class="default" rel="eliminar">
                        <title>Eliminar</title>
                        <app>CADASTRO_FACE</app>
                        <page>TASK</page>
                        <link>LS_TASK</link>
                        <target>alert_submit</target>
                        <img>default|fa-trash-o</img>
                    </item>
                    <item type="specific" code="" class="default" rel="comnts">
                        <title>Comentários</title>
                        <app>CADASTRO_FACE</app>
                        <page>TASK</page>
                        <link>COMMENT_TASK</link>
                        <target>modal</target>
                        <img>default|fa-bars</img>
                    </item>
                </context-menu>
            </table>
        </table_1>
    </content>
</rows>