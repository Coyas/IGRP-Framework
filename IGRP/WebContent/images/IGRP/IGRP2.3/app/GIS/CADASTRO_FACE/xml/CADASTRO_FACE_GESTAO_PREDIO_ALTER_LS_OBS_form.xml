    <content>
        <title>Observação</title>
        <page_obs type="sectionheader" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <page_obs_text type="text" name="p_page_obs_text" persist="true" maxlength="100">
                    <value>
                        <![CDATA[Ver/ Registar Observacao]]>
                    </value>
                </page_obs_text>
            </fields>
        </page_obs>
        <form_obs type="form" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <reg_observacao name="p_reg_observacao" type="textarea" maxlength="4000" required="false" change="false" readonly="false" disabled="false" placeholder="" right="false">
                    <label>Observação</label>
                    <value>Lorem officia labore rem sit perspiciatis sed aperiam amet voluptatem ut aliqua rem ipsum consectetur</value>
