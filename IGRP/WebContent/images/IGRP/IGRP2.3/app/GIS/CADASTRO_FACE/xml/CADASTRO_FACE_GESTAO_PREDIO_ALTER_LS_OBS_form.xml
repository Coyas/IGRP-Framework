                <hidden name="p_id_claim" type="hidden" maxlength="30" tag="id_claim"/>
                <hidden name="p_id_item" type="hidden" maxlength="30" tag="id_item"/>
            </fields>
            <tools-bar>
                <item type="form" code="" class="primary" rel="gravar">
                    <title>Gravar</title>
                    <app>CADASTRO_FACE</app>
                    <page>GESTAO_PREDIO_ALTER</page>
                    <link>LS_OBS</link>
                    <target>submit</target>
                    <img>primary|fa-save</img>
                </item>
            </tools-bar>
        </form_obs>
        <ls_observacao type="table" xml-type="table" gen-type="container" gen-group="true">
            <fields>
                <entidade name="p_entidade" type="text" maxlength="50" align="left" lookup_parser="false">
                    <label>Entidade</label>
                </entidade>
                <data_obs name="p_data_obs" type="text" maxlength="50" align="left" lookup_parser="false">
                    <label>Data</label>
                </data_obs>
                <observacao name="p_observacao" type="text" maxlength="4000" align="left" lookup_parser="false">
