<rows>
    <link_img>images/IGRP/IGRP2.3</link_img>
    <site>
        <welcome_note>Oba</welcome_note>
        <footer_note>2017 - Copyright NOSI</footer_note>
        <user_name>Hexagon</user_name>
        <button type="specific">
            <title>Sair</title>
            <app>DUA</app>
            <page />
            <link>#</link>
            <target>_self</target>
            <img>exit.png</img>
        </button>
    </site>
    <page>ConfigDatabase</page>
    <app>igrp</app>
    <plsql>
        <action>index</action>
        <package_instance></package_instance>
        <package_db>nosi.webapps.igrp.pages</package_db>
        <package_html>ConfigDatabase</package_html>
        <table_name></table_name>
        <with_replace>false</with_replace>
        <with_label>false</with_label>
        <with_biztalk>false</with_biztalk>
        <dynamic_menu>false</dynamic_menu>
        <copy_menu>false</copy_menu>
        <package_copy_db />
        <package_copy_html />
        <preserve_url>webapps?r=igrp/page/preserveUrl&amp;p_id=37</preserve_url>
    </plsql>
    <content>
        <title>Configurar Base de Dados de uma aplicação</title>
        <sectionheader_1 type="sectionheader" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <sectionheader_1_text type="text" name="p_sectionheader_1_text" persist="true" maxlength="4000">
                    <value>Configuração de base de dados</value>
                </sectionheader_1_text>
            </fields>
        </sectionheader_1>
        <toolsbar_1 type="toolsbar" xml-type="toolsbar" gen-type="menu" gen-group="">
            <item type="specific" code="" rel="testar_conexao">
                <title>Testar conexão</title>
                <app>igrp</app>
                <page>ConfigDatabase</page>
                <link>index</link>
                <target>submit</target>
                <img>success|fa-check</img>
                <parameter>undefined/IGRP/images/IGRP/IGRP2.3/app/igrp/configdatabase/ConfigDatabase.xsl</parameter>
            </item>
            <item type="specific" code="" rel="gravar">
                <title>Gravar</title>
                <app>igrp</app>
                <page>ConfigDatabase</page>
                <link>index</link>
                <target>submit</target>
                <img>primary|fa-save</img>
                <parameter>undefined/IGRP/images/IGRP/IGRP2.3/app/igrp/configdatabase/ConfigDatabase.xsl</parameter>
            </item>
        </toolsbar_1>
        <form_1 type="form" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <nome_de_conexao name="p_nome_de_conexao" type="text" maxlength="30" required="true" change="false" readonly="false" disabled="false" placeholder="Ex: DEVNOSI" right="false">
                    <label>Nome de conexão</label>
                    <value>Perspiciatis voluptatem totam</value>
                </nome_de_conexao>
                <aplicacao name="p_aplicacao" type="select" multiple="false" domain="" maxlength="30" required="true" change="false" disabled="false" right="false" java-type="">
                    <label>Aplicação</label>
                    <list>
                        <option/>
                        <option>
                            <text>Option 1</text>
                            <value>1</value>
                        </option>
                        <option>
                            <text>Option 2</text>
                            <value>2</value>
                        </option>
                        <option>
                            <text>Option 3</text>
                            <value>3</value>
                        </option>
                        <option>
                            <text>Option 4</text>
                            <value>4</value>
                        </option>
                    </list>
                </aplicacao>
                <tipo_base_dados name="p_tipo_base_dados" type="select" multiple="false" maxlength="20" required="true" change="true" disabled="false" right="false" domain="" java-type="">
                    <label>Tipo Base Dados</label>
                    <list>
                        <option/>
                        <option>
                            <text>Option 1</text>
                            <value>1</value>
                        </option>
                        <option>
                            <text>Option 2</text>
                            <value>2</value>
                        </option>
                        <option>
                            <text>Option 3</text>
                            <value>3</value>
                        </option>
                        <option>
                            <text>Option 4</text>
                            <value>4</value>
                        </option>
                    </list>
                </tipo_base_dados>
                <config name="p_config" type="separator" maxlength="30" placeholder="" right="false">
                    <label>Config</label>
                </config>
                <hostname name="p_hostname" type="text" maxlength="100" required="true" change="false" readonly="false" disabled="false" placeholder="Ex: nosidev02.gov.cv" right="false">
                    <label>Hostname</label>
                    <value>Rem lorem perspiciatis lorem stract</value>
                </hostname>
                <port name="p_port" type="number" maxlength="30" required="true" change="false" readonly="false" disabled="false" placeholder="" right="false" min="" max="" java-type="">
                    <label>Porta</label>
                    <value>3</value>
                </port>
                <username name="p_username" type="text" maxlength="80" required="true" change="false" readonly="false" disabled="false" placeholder="" right="false">
                    <label>Username</label>
                    <value>Mollit adipiscing aliqua natus iste</value>
                </username>
                <password name="p_password" type="password" maxlength="80" required="false" change="false" readonly="false" disabled="false" placeholder="" right="false">
                    <label>Password</label>
                    <value>******</value>
                </password>
                <nome_de_bade_dados name="p_nome_de_bade_dados" type="text" maxlength="50" required="true" change="false" readonly="false" disabled="false" placeholder="Ex: devnosi.gov.cv" right="false">
                    <label>Nome de Bade Dados</label>
                    <value>Sit accusantium natus sit consectetur</value>
                </nome_de_bade_dados>
            </fields>
        </form_1>
        <paragraph_1 type="paragraph" xml-type="form" title="Example" gen-type="container" gen-group="">
            <fields>
                <paragraph_1_text type="text" name="p_paragraph_1_text" persist="true" maxlength="4000">
                    <value>DEVNOSI =
 (DESCRIPTION = 
 (ADDRESS = (PROTOCOL = TCP) (HOST = nosidev02.gov.cv) (PORT=1521))
(CONNECT_DATA =
     (SERVER = DEDICATED)
     SERVICE_NAME = devnosi.gov.cv)
   )
)</value>
                </paragraph_1_text>
            </fields>
        </paragraph_1>
    </content>
</rows>
