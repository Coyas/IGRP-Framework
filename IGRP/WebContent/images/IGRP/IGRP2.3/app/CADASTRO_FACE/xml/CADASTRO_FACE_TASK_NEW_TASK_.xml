<rows>
    <site>
        <welcome_note>Ola</welcome_note>
        <footer_note>2011 - Copyright NOSI</footer_note>
        <user_name>red-igrp</user_name>
        <button type="specific">
            <title>Sair</title>
            <app>RED</app>
            <page />
            <link>#</link>
            <target>_self</target>
            <img>exit.png</img>
        </button>
    </site>
    <page>TASK</page>
    <app>CADASTRO_FACE</app>
    <plsql>
        <action>NEW_TASK</action>
        <package_instance>devnosi</package_instance>
        <package_db>PRECAD_NEW_TASK_DB</package_db>
        <package_html>PRECAD_NEW_TASK_HTML</package_html>
        <table_name></table_name>
        <with_replace>true</with_replace>
        <with_label>true</with_label>
        <with_biztalk>false</with_biztalk>
        <dynamic_menu>false</dynamic_menu>
        <copy_menu>false</copy_menu>
        <package_copy_db />
        <package_copy_html />
        <preserve_url>https://nosiappsdev.gov.cv/redglobal_dev/red.form_designer_db.preserve_your_code</preserve_url>
    </plsql>
    <content>
        <title>Novo Task</title>
        <toolsbar_1 type="toolsbar" xml-type="toolsbar" gen-type="menu" gen-group="">
            <item type="specific" code="" rel="gaurdar">
                <title>Gaurdar</title>
                <app>CADASTRO_FACE</app>
                <page>TASK</page>
                <link>NEW_TASK</link>
                <target>submit</target>
                <img>primary|fa-check-circle</img>
                <parameter>http://igrp.teste.gov.cv/images/IGRP/IGRP2.3/app/CADASTRO_FACE/xml/CADASTRO_FACE_TASK_NEW_TASK_.xml</parameter>
                <params>primary||fa-check-circle|www</params>
            </item>
        </toolsbar_1>
        <form_1 type="form" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <titulo name="p_titulo" type="text" required="true" change="false" readonly="false" disabled="false" maxlength="30" right="false" placeholder="">
                    <label>Titulo</label>
                    <value>Elit sit accusantium aliqua ipsum</value>
                </titulo>
                <atribuir_a name="p_atribuir_a" type="select" multiple="false" required="true" change="false" disabled="false" right="false" maxlength="30">
                    <label>Atribuir A</label>
                    <list>
                        <option>
                            <text>Option 1</text>
                            <value>1</value>
                        </option>
                        <option>
                            <text>Option 2</text>
                            <value>2</value>
                        </option>
                        <option>
                            <text>Option 3</text>
                            <value>3</value>
                        </option>
                        <option>
                            <text>Option 4</text>
                            <value>4</value>
                        </option>
                    </list>
                </atribuir_a>
                <date_limite name="p_date_limite" type="date" format="IGRP_datePicker" required="false" change="false" readonly="false" disabled="false" maxlength="30" right="false" class="default" placeholder="">
                    <label>Date Limite</label>
                    <value/>
                </date_limite>
                <descricao name="p_descricao" type="textarea" required="false" change="false" readonly="false" disabled="false" maxlength="160" right="false" placeholder="">
                    <label>Descrição</label>
                    <value>Omnis sit sed voluptatem officia rem officia iste anim ipsum rem laudantium consectetur magna totam</value>
                </descricao>
                <hidden name="p_id" type="hidden" maxlength="30" tag="id"/>
            </fields>
        </form_1>
    </content>
</rows>