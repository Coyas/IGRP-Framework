        <!--START PAGE--><form action="#" method="post" id="formular_default" name="formular_default" class="default_form"  enctype="multipart/form-data"><!--FORMULARIO --><!--START FORM--><!--TOOL BAR--><xsl:apply-templates mode="tools-bar" select="rows/content/form/tools-bar" /><div class="box-content"><!--HIDDEN FIELDS--><xsl:apply-templates mode="form-hidden" select="rows/content/form/value" /><!--Devo abrir uma div aqui --><div id="startPage"><xsl:if test="rows/content/form/label/cliente"><label><span>(*) <xsl:value-of select="rows/content/form/label/cliente"/></span><input type="text" name="{rows/content/form/value/cliente/@name}" value="{rows/content/form/value/cliente}"   maxlength="30"  class="required"><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/cliente" /></xsl:call-template></input><xsl:call-template name="lookup-tool"><xsl:with-param name="page" select="rows/page" /><xsl:with-param name="ad_hoc" select="'1'" /><xsl:with-param name="action" select="'LOOKUP'" /><xsl:with-param name="name" select="rows/content/form/value/cliente/@name" /><xsl:with-param name="js_lookup" select="rows/content/form/lookup/cliente" /></xsl:call-template></label></xsl:if><xsl:if test="rows/content/form/label/codigo"><label><span>(*) <xsl:value-of select="rows/content/form/label/codigo"/></span><input type="text" name="{rows/content/form/value/codigo/@name}" value="{rows/content/form/value/codigo}"   maxlength="30"  class="required"><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/codigo" /></xsl:call-template></input></label></xsl:if><xsl:if test="rows/content/form/label/dt_inicio"><label><span><xsl:value-of select="rows/content/form/label/dt_inicio"/></span><input type="text" name="{rows/content/form/value/dt_inicio/@name}" value="{rows/content/form/value/dt_inicio}"  class="IGRP_datepicker date"><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/dt_inicio" /></xsl:call-template></input></label></xsl:if><xsl:if test="rows/content/form/label/dt_fim"><label><span><xsl:value-of select="rows/content/form/label/dt_fim"/></span><input type="text" name="{rows/content/form/value/dt_fim/@name}" value="{rows/content/form/value/dt_fim}"  class="IGRP_datepicker date"><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/dt_fim" /></xsl:call-template></input></label></xsl:if><xsl:if test="rows/content/form/label/notas"><label><span><xsl:value-of select="rows/content/form/label/notas"/></span><input type="text" name="{rows/content/form/value/notas/@name}" value="{rows/content/form/value/notas}"   maxlength="30"  class=""><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/notas" /></xsl:call-template></input></label></xsl:if></div><!--FIM DO SEPARADOR--><!--INICIO DO SEPARADOR "ITEMS"--><div id="{local-name(rows/content/form/label/items)}"><xsl:if test="rows/content/form/label/items"><xsl:call-template name="page-title"><xsl:with-param name="title" select="rows/content/form/label/items" /><xsl:with-param name="class" select="'subtitle'" /></xsl:call-template></xsl:if><xsl:if test="rows/content/form/label/items"><xsl:call-template name="tools-bar-form-group"><xsl:with-param name="rel" select="'items'" /></xsl:call-template></xsl:if><xsl:if test="rows/content/form/label/dt_inicio_item"><label><span><xsl:value-of select="rows/content/form/label/dt_inicio_item"/></span><input type="text" name="{rows/content/form/value/dt_inicio_item/@name}" value="{rows/content/form/value/dt_inicio_item}"  rel="F_items"  class="IGRP_datepicker date"><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/dt_inicio_item" /></xsl:call-template></input></label></xsl:if><xsl:if test="rows/content/form/label/dt_fim_item"><label><span><xsl:value-of select="rows/content/form/label/dt_fim_item"/></span><input type="text" name="{rows/content/form/value/dt_fim_item/@name}" value="{rows/content/form/value/dt_fim_item}"  rel="F_items"  class="IGRP_datepicker date"><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/dt_fim_item" /></xsl:call-template></input></label></xsl:if><xsl:if test="rows/content/form/label/tp_consumo"><label><span><xsl:value-of select="rows/content/form/label/tp_consumo"/></span><select name="{rows/content/form/list/tp_consumo/@name}" rel="F_items"  class="IGRP_change"><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/tp_consumo" /></xsl:call-template><xsl:for-each select="rows/content/form/list/tp_consumo/option"><option value="{value}"><xsl:if test="@selected='true'"><xsl:attribute name="selected">selected</xsl:attribute></xsl:if><xsl:value-of select="text"/></option></xsl:for-each></select></label></xsl:if><xsl:if test="rows/content/form/label/servico"><label><span><xsl:value-of select="rows/content/form/label/servico"/></span><input type="text" name="{rows/content/form/value/servico/@name}" value="{rows/content/form/value/servico}"  rel="F_items"   maxlength="30"  class=""><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/servico" /></xsl:call-template></input><xsl:call-template name="lookup-tool"><xsl:with-param name="page" select="rows/page" /><xsl:with-param name="ad_hoc" select="'1'" /><xsl:with-param name="action" select="'LOOKUP'" /><xsl:with-param name="name" select="rows/content/form/value/servico/@name" /><xsl:with-param name="js_lookup" select="rows/content/form/lookup/servico" /></xsl:call-template></label></xsl:if><xsl:if test="rows/content/form/label/quota"><label><span><xsl:value-of select="rows/content/form/label/quota"/></span><input type="text" name="{rows/content/form/value/quota/@name}" value="{rows/content/form/value/quota}"  rel="F_items"   maxlength="30"  class=""><xsl:call-template name="FIELD_validator"><xsl:with-param name="field" select="rows/content/form/label/quota" /></xsl:call-template></input></label></xsl:if><xsl:if test="rows/content/form/label/items"><!--INICIO DA TABELA  rows/content/form/table/items/--><div class="simple-table"><table rel="T_items"><!--CABECALHO DA TABELA --><thead><tr><xsl:if test="rows/content/form/table/items/label/dt_inicio_item"><xsl:if test="not(rows/content/form/table/items/label/dt_inicio_item/@visible)"><th  ><xsl:value-of select="rows/content/form/table/items/label/dt_inicio_item"/></th></xsl:if></xsl:if><xsl:if test="rows/content/form/table/items/label/dt_fim_item"><xsl:if test="not(rows/content/form/table/items/label/dt_fim_item/@visible)"><th  ><xsl:value-of select="rows/content/form/table/items/label/dt_fim_item"/></th></xsl:if></xsl:if><xsl:if test="rows/content/form/table/items/label/tp_consumo"><xsl:if test="not(rows/content/form/table/items/label/tp_consumo/@visible)"><th  ><xsl:value-of select="rows/content/form/table/items/label/tp_consumo"/></th></xsl:if></xsl:if><xsl:if test="rows/content/form/table/items/label/servico"><xsl:if test="not(rows/content/form/table/items/label/servico/@visible)"><th  ><xsl:value-of select="rows/content/form/table/items/label/servico"/></th></xsl:if></xsl:if><xsl:if test="rows/content/form/table/items/label/quota"><xsl:if test="not(rows/content/form/table/items/label/quota/@visible)"><th  ><xsl:value-of select="rows/content/form/table/items/label/quota"/></th></xsl:if></xsl:if><th align="center" width="10"></th><th align="center" width="10"></th></tr></thead><!--CORPO DA TABELA--><tbody><xsl:for-each select="rows/content/form/table/items/value/row[not(@total='yes')]"><tr><xsl:if test="dt_inicio_item"><xsl:if test="not(dt_inicio_item/@visible)"><td><xsl:value-of select="dt_inicio_item_desc" /><input type="hidden" name="p_dt_inicio_item_fk" value="{dt_inicio_item}" /><input type="hidden" name="p_dt_inicio_item_fk_desc" value="{dt_inicio_item_desc}" /></td></xsl:if></xsl:if><xsl:if test="dt_inicio_item/@visible='false'"><input type="hidden" name="p_dt_inicio_item_fk" value="{dt_inicio_item}" /><input type="hidden" name="p_dt_inicio_item_fk_desc" value="{dt_inicio_item_desc}" /></xsl:if><xsl:if test="dt_fim_item"><xsl:if test="not(dt_fim_item/@visible)"><td><xsl:value-of select="dt_fim_item_desc" /><input type="hidden" name="p_dt_fim_item_fk" value="{dt_fim_item}" /><input type="hidden" name="p_dt_fim_item_fk_desc" value="{dt_fim_item_desc}" /></td></xsl:if></xsl:if><xsl:if test="dt_fim_item/@visible='false'"><input type="hidden" name="p_dt_fim_item_fk" value="{dt_fim_item}" /><input type="hidden" name="p_dt_fim_item_fk_desc" value="{dt_fim_item_desc}" /></xsl:if><xsl:if test="tp_consumo"><xsl:if test="not(tp_consumo/@visible)"><td><xsl:value-of select="tp_consumo_desc" /><input type="hidden" name="p_tp_consumo_fk" value="{tp_consumo}" /><input type="hidden" name="p_tp_consumo_fk_desc" value="{tp_consumo_desc}" /></td></xsl:if></xsl:if><xsl:if test="tp_consumo/@visible='false'"><input type="hidden" name="p_tp_consumo_fk" value="{tp_consumo}" /><input type="hidden" name="p_tp_consumo_fk_desc" value="{tp_consumo_desc}" /></xsl:if><xsl:if test="servico"><xsl:if test="not(servico/@visible)"><td><xsl:value-of select="servico_desc" /><input type="hidden" name="p_servico_fk" value="{servico}" /><input type="hidden" name="p_servico_fk_desc" value="{servico_desc}" /></td></xsl:if></xsl:if><xsl:if test="servico/@visible='false'"><input type="hidden" name="p_servico_fk" value="{servico}" /><input type="hidden" name="p_servico_fk_desc" value="{servico_desc}" /></xsl:if><xsl:if test="quota"><xsl:if test="not(quota/@visible)"><td><xsl:value-of select="quota_desc" /><input type="hidden" name="p_quota_fk" value="{quota}" /><input type="hidden" name="p_quota_fk_desc" value="{quota_desc}" /></td></xsl:if></xsl:if><xsl:if test="quota/@visible='false'"><input type="hidden" name="p_quota_fk" value="{quota}" /><input type="hidden" name="p_quota_fk_desc" value="{quota_desc}" /></xsl:if><td align="center" width="10"><input type="hidden" name="p_items_id" value="{items_id}" /><xsl:if test="not(@nodelete)"><img alt="apagar" src="{$path}/img/icon/ctx-delete.png" class="IGRP_delRow" rel="items"/></xsl:if></td><td align="center" width="10"><xsl:if test="not(@noupdate)"><img alt="editar" src="{$path}/img/icon/edit.png" class="IGRP_editRow" rel="items"/></xsl:if></td></tr></xsl:for-each></tbody></table><!--LEGENDA FINAL DA TABELA --><xsl:apply-templates mode="legend_color" select="rows/content/form/table/items/legend_color"/></div><!--FIM DA TABELA--></xsl:if></div><!-- "FORM" devo fachar DIV_SEPARATOR aqui--></div><!--END FORM--></form><!--END PAGE-->


