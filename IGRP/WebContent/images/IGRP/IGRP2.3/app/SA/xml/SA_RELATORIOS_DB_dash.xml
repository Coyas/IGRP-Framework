    <content>
        <title>Dashbord</title>
        <form_1 type="form" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <periodo name="p_periodo" type="radiolist" required="false" change="false" readonly="false" disabled="false" right="false" maxlength="30">
                    <label>Periodo</label>
                    <list>
                        <option>
                            <text>Option 1</text>
                            <value>1</value>
                        </option>
                        <option>
                            <text>Option 2</text>
                            <value>2</value>
                        </option>
                        <option>
                            <text>Option 3</text>
                            <value>3</value>
                        </option>
                        <option>
                            <text>Option 4</text>
                            <value>4</value>
                        </option>
                    </list>
                </periodo>
                <organica name="p_organica" type="select" multiple="true" required="false" change="false" disabled="false" right="false" maxlength="30">
                    <label>Orgânica</label>
                    <list>
                        <option>
                            <text>Option 1</text>
                            <value>1</value>
                        </option>
                        <option>
                            <text>Option 2</text>
                            <value>2</value>
                        </option>
                        <option>
                            <text>Option 3</text>
                            <value>3</value>
                        </option>
                        <option>
                            <text>Option 4</text>
                            <value>4</value>
                        </option>
                    </list>
                </organica>
                <opcao_filtro name="p_opcao_filtro" type="radiolist" required="false" change="false" readonly="false" disabled="false" right="false" maxlength="30">
                    <label>Opção de Filtro</label>
                    <list>
                        <option>
                            <text>Option 1</text>
                            <value>1</value>
                        </option>
                        <option>
                            <text>Option 2</text>
                            <value>2</value>
                        </option>
                        <option>
                            <text>Option 3</text>
                            <value>3</value>
                        </option>
                        <option>
                            <text>Option 4</text>
                            <value>4</value>
                        </option>
                    </list>
                </opcao_filtro>
            </fields>
            <tools-bar>
                <item type="form" code="" class="info" rel="filtro">
                    <title>Filtro</title>
                    <app>SA</app>
                    <page>RELATORIOS</page>
                    <link>DB</link>
                    <target>submit</target>
                    <img>info|fa-filter</img>
                </item>
                <item type="form" code="" class="success" rel="avancadas">
                    <title>Avançadas</title>
                    <app>SA</app>
                    <page>RELATORIOS</page>
                    <link>AV</link>
                    <target>modal</target>
                    <img>success|fa-line-chart</img>
                </item>
            </tools-bar>
        </form_1>
        <total_atende type="statbox" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <total_atende_tit name="p_total_atende_tit" type="text" maxlength="300" persist="true">
                    <label>Box Title</label>
                    <value>Total De Atendimento</value>
                </total_atende_tit>
                <total_atende_val name="p_total_atende_val" type="text" maxlength="300" persist="true">
                    <label>Value</label>
                    <value>895</value>
                </total_atende_val>
                <total_atende_txt name="p_total_atende_txt" type="text" maxlength="300" persist="true">
                    <label>Url Text</label>
                    <value>Mais...</value>
                </total_atende_txt>
                <total_atende_url name="p_total_atende_url" type="text" maxlength="300" persist="true">
                    <label>Url</label>
                    <value>www.google.cv</value>
                </total_atende_url>
                <total_atende_bg name="p_total_atende_bg" type="text" maxlength="300" persist="true">
                    <label>Background</label>
                    <value>cp-cyan</value>
                </total_atende_bg>
                <total_atende_icn name="p_total_atende_icn" type="text" maxlength="300" persist="true">
                    <label>Icon</label>
                    <value>fa-group</value>
                </total_atende_icn>
            </fields>
        </total_atende>
