        <user_name>red-igrp</user_name>
        <button type="specific">
            <title>Sair</title>
            <app>RED</app>
            <page />
            <link>#</link>
            <target>_self</target>
            <img>exit.png</img>
        </button>
    </site>
    <page>SA.SA_T_SERVICOS_CONFIG</page>
    <app>SA</app>
    <plsql>
        <action>FORM</action>
        <package_instance></package_instance>
        <package_db>SA_SERVICOS_CONFIG_FORM_DB</package_db>
        <package_html>SA_SERVICOS_CONFIG_FORM_HTML</package_html>
        <table_name></table_name>
        <with_replace>false</with_replace>
        <with_label>true</with_label>
        <with_biztalk>false</with_biztalk>
        <dynamic_menu>false</dynamic_menu>
        <copy_menu>false</copy_menu>
        <package_copy_db />
        <package_copy_html />
        <preserve_url>https://nosiappsdev.gov.cv/redglobal_dev/red.form_designer_db.preserve_your_code</preserve_url>
    </plsql>
    <content>
        <title>Form</title>
        <page_title type="sectionheader" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <page_title_text type="text" name="p_page_title_text" persist="true" maxlength="100">
                    <value>
                        <![CDATA[Configuração]]>
                    </value>
                </page_title_text>
            </fields>
        </page_title>
        <fmn type="toolsbar" xml-type="toolsbar" gen-type="menu" gen-group="">
            <item type="specific" code="" rel="gravar">
                <title>Gravar</title>
                <app>SA</app>
                <page>SA.SA_T_SERVICOS_CONFIG</page>
                <link>FORM</link>
                <target>submit</target>
                <img>success|fa-save</img>
                <params>success||fa-save|www</params>
            </item>
            <item type="specific" code="" rel="voltar">
                <title>Voltar</title>
                <app>SA</app>
                <page>SA.SA_T_SERVICOS_CONFIG</page>
                <link>BULK</link>
                <target>_self</target>
                <img>info|fa-arrow-left</img>
                <params>info||fa-arrow-left|www</params>
            </item>
        </fmn>
        <form_1 type="form" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <servico name="p_servico" type="lookup" required="true" maxlength="300" readonly="true" action="FORM" page="SA.SA_T_SERVICOS_CONFIG" app="SA" class="default" change="false" disabled="false" right="false">
                    <label>Serviço</label>
                    <value/>
                    <lookup>http://igrp.teste.gov.cv/images/IGRP.bootstrap/app/BOOTSTRAP/xml/lookup.test.xml</lookup>
                </servico>
                <id_organica name="p_id_organica" type="select" required="true" multiple="false" change="false" disabled="false" right="false" maxlength="30">
                    <label>Orgánica</label>
                    <list>
                        <option>
                            <text>Option 1</text>
                            <value>1</value>
                        </option>
                        <option>
                            <text>Option 2</text>
                            <value>2</value>
                        </option>
                        <option>
                            <text>Option 3</text>
                            <value>3</value>
                        </option>
                        <option>
                            <text>Option 4</text>
                            <value>4</value>
                        </option>
                    </list>
                </id_organica>
                <cor name="p_cor" type="color" maxlength="50" format="hex" required="false" change="false" readonly="false" disabled="false" right="false">
                    <label>Cor</label>
                    <value>#6a4b0d</value>
                </cor>
                <imagem name="p_imagem" type="file" required="false" disabled="false" right="false" class="info" maxlength="300">
                    <label>Imagem</label>
                    <value/>
                </imagem>
                <link name="p_link" type="link" maxlength="4000" target="_blank" right="false" class="success" iconColor="#333" iconClass="" img="fa-image">
                    <label>Ver Imagem</label>
                    <value>http://ame.net/cedvefpuf</value>
                </link>
                <hidden name="p_id" type="hidden" tag="id"/>
                <hidden name="p_id_servico" type="hidden" tag="id_servico"/>
                <hidden name="p_tipo_menu" type="hidden" tag="tipo_menu"/>
                <hidden name="p_user_insert" type="hidden" tag="user_insert"/>
                <hidden name="p_data_insert" type="hidden" tag="data_insert"/>
                <hidden name="p_user_update" type="hidden" tag="user_update"/>
                <hidden name="p_data_update" type="hidden" tag="data_update"/>
                <hidden name="p_hidde" type="hidden" tag="hidde"/>
                <hidden name="p_id_organica_old" type="hidden" tag="id_organica_old"/>
            </fields>
        </form_1>
    </content>
</rows>